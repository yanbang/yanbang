import request from '@/utils/request';

const api = {
    servicejkdOltzhuanti: '/ZhyJkdCon/servicejkdOltzhuanti', //  olt专题
    servicejkdFtthzhuanti: '/ZhyJkdCon/servicejkdFtthzhuanti', //  ftth专题
    servicejkdFttharray: '/ZhyJkdCon/servicejkdFttharray', //  ftth区县劣化占比排名
}

var serviceApi = {}
Object.keys(api).forEach(key => {
    serviceApi[key] = function(parameter) {
        return request({
            url: api[key],
            method: 'post',
            data: parameter
        })
    }
})

export default serviceApi