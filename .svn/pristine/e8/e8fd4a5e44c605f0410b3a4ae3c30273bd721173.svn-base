    },
    dialogLineChart(id) {
      let data = {
        id: id,
        lable: id == 'dialogMouthChart' ? '月度趋势' : '年度趋势',
        xAxis: this.diaxAxis1,
        data1: this.diaLine3,
        data2: this.diaLine4
      }
      leftLine.initChart(data)
