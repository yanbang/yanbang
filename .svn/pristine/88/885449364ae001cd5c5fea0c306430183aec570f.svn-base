          charts3.value = keyarr.map(key => {
             return orderData1.map(item => {
                  return item[key]
              })
          })
          charts3.bar_markLine = [this.averageData.zj_intime_rate, this.averageData.zj_linlk_intime_rate, this.averageData.zj_hurry_rate ]
        } else  if(tabstatus == '2') {
          // 修机
          var orderData2 = this.orderDataFn(ftphList, 'gz_hurry_rate');
          charts3.Xdata = orderData2.map(item => {
            return item.name
          });
          var keyarr = [ 'gz_linlk_intime_rate', 'gz_hurry_rate', 'gz_cf_rate']
          charts3.names = [ '联系客户及时率', '催单率', '7天重复障碍率'],
          charts3.value = keyarr.map(key => {
             return orderData2.map(item => {
                  return item[key]
              })
          })
          charts3.bar_markLine = [this.averageData.gz_linlk_intime_rate, this.averageData.gz_hurry_rate, this.averageData.gz_cf_rate ]
        } 
        echarts_init.initechart_lines(charts3);

        if (type && type == 'change_status') {
          return;
        };

        var params1_orderData = this.orderDataFn(ftphList, 'kd_xz_rate');
        var params1_orderData_yj =  this.orderDataFn(ftphList, 'kd_yz_rate');

        this.zhyData.name2 = params1_orderData[0].name;
        this.zhyData.value2 = params1_orderData[0].kd_xz_rate;

         this.zhyData.name3 = params1_orderData_yj[0].name;
        this.zhyData.value3 = params1_orderData_yj[0].kd_yz_rate;

        let charts = {
            domId: 'chart_line1',
            unit: '百分比',
             zoomVal: 100,
            names: ['装机', '移机'],
            lineX: params1_orderData.map(item => {
              return item.name
            }),
            bar_markLine: [this.averageData.kd_xz_rate, this.averageData.kd_yz_rate],
            value: [
                params1_orderData.map(item => {
                  return item.kd_xz_rate
                }),
                params1_orderData.map(item => {
                  return item.kd_yz_rate
                })
            ]

          }
          echarts_init.initechart_lines(charts)

          var params2_orderData = this.orderDataFn(ftphList, 'kd_fz_rate');
          this.zhyData.avg1 = this.averageData.kd_fz_rate;
          this.zhyData.name1 = params2_orderData[0].name;
          this.zhyData.value1 = params2_orderData[0].kd_fz_rate;
          let charts2 = {
            domId: 'chart_line2',
            unit: '百分比',
             zoomVal: 100,
            names: ['宽带发展', '固话发展', '移网发展'],
            lineX: params2_orderData.map(item => {
              return item.name
            }),
            value: [
               params2_orderData.map(item => {
                  return item.kd_fz_rate
                }),
                params2_orderData.map(item => {
                  return item.gh_fz_rate
                }),
                params2_orderData.map(item => {
                  return item.yw_fz_rate
                }),
            ],
           bar_markLine: [this.averageData.kd_fz_rate, this.averageData.gh_fz_rate, this.averageData.yw_fz_rate],

          }
          echarts_init.initechart_lines(charts2);
      },
      

      getList() {
         var cityCode = this.cityCode
         efficacyList(cityCode).then(response => {
            var datal = response.filter(item => {
              return item.code != '0000'
            })
            var avdatal = response.filter(item => {
              return item.code == '0000'
            })
            this.ftphList = datal;
            this.averageData = avdatal.length > 0 ? avdatal[0] : {}
            this.render_wcxnTable()
            this.render_ztzjTable()
            this.render_csjsTable()
