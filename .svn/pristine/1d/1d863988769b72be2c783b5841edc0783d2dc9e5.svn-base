<script>
import Header from '@/components/resource_visual/header2'
import Footer from '@/components/resource_visual/footer'
import echarts_init from '../fu/echart_lines'
import vueSeamlessScroll from 'vue-seamless-scroll'
import store from 'store'
import { mapState } from 'vuex'
import serviceApi from '@/api/network/wireless'
var Qs = require('@/utils/qs.js')
var _ = require('@/utils/lodash.js')
import { warnList } from '@/api/broadband/warn'
import waterPoloChart from '@/views/resource_visual/echarts_component/waterPoloChart'
import serviceApi_fu from '@/api/fu_serviceApi'
export default {
  data() {
    return {

      isDialogShow_ZHY: false,
      dloading: false,
      isDialogShow: false,
      rt1status: 2,
      rt1status2: 2,
      gisTitle: '山东省',
      cityCode: '370000',
      iframeSrc: '',
      gisQueryParams: {},
      queryParams: {},
      gisMapData: {},
      mapLevel: 1,
      areaCode: '',
      control: [],
      initgisTitle: '',
      totalData: {},
      tabdata2: [
        { title: '满意度', active: false, href: '/unicomwisdomeyes/manyidu?pagesource=wldc' },
        { title: '投申诉', active: true, href: '/unicomwisdomeyes/toushensu?pagesource=wldc' }
      ],
    }
  },
  components: {
    Header,
    Footer,
    vueSeamlessScroll
  },
  created() {
    self = this
  },
  mounted() {
    var self = this
    var initArea_id = '370000'
    this.mapLevel = 1
    if (store.get('userInfo')) {
      var userInfo =  store.get('userInfo')
      var mapLevel = userInfo.levelAreaLevel;
      if (mapLevel == '1') {
        // 省级用户
        this.control = ['370000']
      } else if (mapLevel >= '2') {
        this.control = ['370000', userInfo.levelAreaCode]
      }
    };
    this.cityCode = initArea_id
    this.initGisMap(initArea_id)
    // 注册地图事件
    this.initgisTitle = this.gisTitle + ''
    window.addEventListener('message', self.gismapListener, true)
    this.serverInit()
  },
  watch: {

  },
  computed: {
    ...mapState({
      searchDate: state => state.resource.searchDate
    }),
   
  },

  methods: {
    serverInit() {
      this.serviceFuTss()
    },

     serviceFuTss() {
      var param = {
        cityCode: this.cityCode
      }
      serviceApi_fu.serviceFuTss(param).then(res => {
       
        if (res.rspCode == '0000') {
          
             var datas = res.result.data || []
              var datal = datas.filter(item => {
                return item.code != this.cityCode
              })
              var sdData = datas.filter(item => {
                return item.code == this.cityCode
              })[0] || {}
              this.totalData = sdData
              datal.forEach(item => {
                Object.keys(item).forEach(key => {
                  Number(item[key]) ? (item[key] = Number(item[key])) : item[key]
                })
              })
               
              this.initEchartFn(datal)
            } else {
          this.$message.error(res.rspMsg)
        }
      })
    },
    initEchartFn (datal) {
      var datal1 = this.orderDataFn(datal, 'tousu_nums2')
      var chartParams = {
        domId: 'chart_line1',
          zoomVal: 100,
        legend: ['本月', '上月'],
        xData: datal1.map(item => {
          return item.name
        }),
        barData: datal1.map(item => {
            return item.tousu_nums2 || 0
          }),
        lineData: datal1.map(item => {
            return item.tousu_nums1 || 0
          }),  
      };
      echarts_init.initEchart_barLine_y(chartParams)

       var datal2 = this.orderDataFn(datal, 'tousu_nums4')
      var chartParams2 = {
        domId: 'chart_line2',
          zoomVal: 100,
        legend: ['本月', '上月'],
        xData: datal2.map(item => {
          return item.name
        }),
        barData: datal2.map(item => {
            return item.tousu_nums4 || 0
          }),
        lineData: datal2.map(item => {
            return item.tousu_nums3 || 0
          }),  
      };
      echarts_init.initEchart_barLine_y(chartParams2)
      
      var datal3 = this.orderDataFn(datal, 'yw_wh_tsu_lv')
      var chartParams3 = {
        domId: 'chart_line3',
          zoomVal: 100,
        name: '本月移网万户投诉率',
        uni: '%',
        xData: datal3.map(item => {
          return item.name
        }),
        color: 'rgb(222,131,69)',
        data: datal3.map(item => {
            return item.yw_wh_tsu_lv || 0
          }), 
      };
      echarts_init.initEchart_singLine(chartParams3)


      var datal4 = this.orderDataFn(datal, 'yw_chtsu_lv')
      var chartParams4 = {
        domId: 'chart_line4',
         zoomVal: 100,
        name: '本月移网万户投诉率',
        uni: '%',
        xData: datal4.map(item => {
          return item.name
        }),
        color: 'rgb(222,131,69)',
        data: datal4.map(item => {
            return item.yw_chtsu_lv || 0
          }), 
      };
      echarts_init.initEchart_singLine(chartParams4)


      

       var datal5 = this.orderDataFn(datal, 'yw_leiji_shensu')
       var chartParams5 = {
        domId: 'chart_line5',
            zoomVal: 100,
        legend: [''],
        xData: datal5.map(item => {
          return item.name
        }),
        barData: datal5.map(item => {
            return item.yw_leiji_shensu || 0
          }), 
      };
      echarts_init.initEchart_barLine_y(chartParams5)


      
    },

    initSortTabledata(datas) {
      var datal = _.cloneDeep(datas)
      datal.forEach(item => {
        Object.keys(item).forEach(key => {
          Number(item[key]) || Number(item[key]) === 0 ? (item[key] = Number(item[key])) : item[key]
        })
      })

      return datal
    },

    //不能删
    orderDataFn(datas, key) {
      return _.orderBy(
        datas,
        function(item) {
          return Number(item[key]) || 0
        },
        ['desc']
      )
    },


    gismapListener(e) {
      var self = this
      if (e.data.message == 'levelSwitch') {
        self.gisTitle = e.data.area_name || this.initgisTitle
        self.mapLevel = e.data.level
        self.areaCode = e.data.area_id
        self.gisMapData = e.data
        self.gisQueryParams = Object.assign({}, self.gisQueryParams, e.data)
        self.gisQueryParams.area_name = ''
        console.log('message', e.data)
        self.cityCode = e.data.area_id || '370000'
        this.serverInit()
      } 
      //  else if (e.data.message == 'changeCity') {
      //    self.gisTitle = e.data.area_name || '山东省'
      //    self.mapLevel = e.data.level
      //    self.areaCode = e.data.area_id;
      //    self.gisMapData = e.data;
      //    self.gisQueryParams = Object.assign({},  self.gisQueryParams, e.data)
      //    self.gisQueryParams.area_name = '';
      //    self.cityCode = e.data.area_id;
      //    self.serverInit()
      // }
    },
    initGisMap(initArea_id) {
      // 初始化地图参数
      this.gisQueryParams = {
        control: this.control.join(','),
        level: this.mapLevel,
        sceneid: '012',
        gis_sceneid: '012',
        area_id: initArea_id || '370000',
        poi_city_id: '',
        system_type: 'C40',
        login_no: store.get('staffId') || '',
        timest: this.$store.state.resource.searchDate
      }
      var gisQueryParams = JSON.parse(JSON.stringify(this.gisQueryParams))
      this.iframeSrc = window.gis_iframeSrc + `/smartEyes?${Qs.stringify(gisQueryParams)}`
    }
  },
  destroyed() {
    // 在组件生命周期结束的时候销毁。
    window.removeEventListener('message', this.gismapListener, true)
  }
}
</script>

