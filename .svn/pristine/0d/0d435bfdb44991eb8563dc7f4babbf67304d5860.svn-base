            lineData: orderData3.map(item => {
              return item.yewu_4g
            })
          }
          echarts_init.initEchart_barLine(chartParams3)
          // PRB利用率
          var datal4 = response.filter(item => {
            return item.code != '370000'
          })
          var avrData4 =
            response.filter(item => {
              return item.code == '370000'
            })[0] || {}

          var orderData4 = this.orderDataFn(datal4, 'prb_5g')
          var orderData_lldk = this.orderDataFn(datal4, 'no_port_nums')
          this.zhyData.name4 = orderData_lldk[0].name
          this.zhyData.value4 = orderData_lldk[0].no_port_nums

          this.zhyData.name4 = orderData4[0].name
          this.zhyData.value4 = orderData4[0].no_fgq_nums

          var chartParams4 = {
