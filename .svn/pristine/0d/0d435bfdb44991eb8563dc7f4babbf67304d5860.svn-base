      serviceApi.serviceWwxDpzb(param).then(res => {
        if (res.rspCode == '0000') {
          var response = res.result.data || []

          //5g全生命周期管理
          var datal = response.filter(item => {
            return item.code != '370000'
          })
          var avrData =
            response.filter(item => {
              return item.code == '370000'
            })[0] || {}

          var orderData = this.orderDataFn(datal, 'rate_rw')
          this.zhyData.name1 = orderData[0].name
          this.zhyData.value1 = orderData[0].olt_retrun_rate + '%'
          var chartParams = {
            domId: 'chart_line1',
            zoomVal: 100,
