    initSortTabledata (datas, nokey) {
      var datal= _.cloneDeep(datas);
       datal.forEach(item => {
       
        Object.keys(item).forEach(key => {
            if (key != nokey) {
              Number(item[key]) || Number(item[key]) === 0 ? item[key] =  Number(item[key]) : item[key];
            }
            
        })
      });
      return datal
