          self.gisTitle = e.data.area_name ||  this.initgisTitle;
          self.mapLevel = e.data.level
          self.areaCode = e.data.area_id;
          self.gisMapData = e.data;
          self.gisQueryParams = Object.assign({}, self.gisQueryParams, e.data)
          self.gisQueryParams.area_name = '';
          console.log('message', e.data)
          self.cityCode = (e.data.area_id || '370000')
          self.initService()
        }
      },
      initGisMap(initArea_id,gisSceneid) {
        // 初始化地图参数
        this.gisQueryParams = {
          control: this.control.join(','),
          level: this.mapLevel,
          sceneid: '08',
          gis_sceneid: gisSceneid,//'0801',
          area_id: initArea_id || '370000',
          poi_city_id: '',
          system_type: 'C40',
          login_no: store.get('staffId') || '',
          timest: this.$store.state.resource.searchDate
        };
        var gisQueryParams = JSON.parse(JSON.stringify(this.gisQueryParams))
        this.iframeSrc = window.gis_iframeSrc +  `/smartEyes?${Qs.stringify(gisQueryParams)}`;

      },
    initEchart (type) {
      var datal = this.gw_leftdata;
      var datal2 = this.jdhdata;
      if (type == 1) {
        var param1 = {
          domId: 'pieChartgu1',
          name:'端业不匹配率',
          unit: '%',
          value: datal.bplv_gm || 0
        }
         echartInit.initChartgauge(param1)

        var param2 = {
          domId: 'pieChartgu2',
          name:'端业不匹配率',
          unit: '%',
          value: datal.lybplv_gm || 0
        }
         echartInit.initChartgauge(param2)

        var param4 = {
          domId: 'pieChartgu4',
          name:'端业不匹配率',
          unit: '%',
          value: datal.pon_lan_bp_lv || 0
        }
         echartInit.initChartgauge(param4)
      } else if (type == 2) {
        var param3 = {
          domId: 'pieChartgu3',
          name:'端业不匹配率',
          unit: '%',
          value: parseFloat( datal2.bp_rate).toFixed(2)|| 0
        }
         echartInit.initChartgauge(param3)
      }
        

       


 


    
   }
  },
  updated() {
     var self = this;
  },
  destroyed() { // 在组件生命周期结束的时候销毁。
    window.removeEventListener('message',this.gismapListener, true)
  }
}
