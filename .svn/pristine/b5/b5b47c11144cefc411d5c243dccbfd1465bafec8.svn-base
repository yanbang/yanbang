        this.tableCol = [
          {filed: 'name', title: '地市'},
         
           {filed: 'ly_gm', title: '路由器不匹配数', sortable: true},
           {filed: 'lybplv_gm', title: '路由器不匹配率', sortable: true, unit: '%'},
        ]
      }  else if (type == '7') {
         this.comeGwTable = this.jdhTableData
