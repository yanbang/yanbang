      var dataList = []
      var bzhd_tableData = this.bzhd_tableData
      var bzhd_checkList = this.bzhd_checkList
      bzhd_tableData.forEach(item => {
        if (bzhd_checkList.indexOf(item.activity_state) > -1) {
          dataList.push(item)
        }
      })
      return dataList
    }
  },
