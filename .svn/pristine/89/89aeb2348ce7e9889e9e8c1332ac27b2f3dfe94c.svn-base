      teamCol: [],
      teamtitle: '',
      cateTeamData: {},
      cateTeamDataDetail: [],
      zhy_content: ''
      
    }
  },
  components: {
    Header,
    Footer,
   
  },
  created() {
    self = this;
  },
  mounted () {
       var self = this;
      var initArea_id = '370000'
      this.mapLevel = 1;
     if (store.get('userInfo')) {
      var userInfo = store.get('userInfo')
      var mapLevel = userInfo.levelAreaLevel
      if (mapLevel == '1') {
          // 省级用户
          this.control = ['370000']
        } else if (mapLevel >= '2') {
          this.control = ['370000', userInfo.levelAreaCode]
        }
     }
      this.cityCode = initArea_id;
      this.initGisMap(initArea_id);
      // 注册地图事件
      this.initgisTitle = this.gisTitle + '';
      window.addEventListener('message', self.gismapListener, true)
     this.initService()
    
  },
  watch: {
    
  },
  computed: {
    ...mapState({
       searchDate: state => state.resource.searchDate,
    }),
   
    
  },
  methods: {
    initService () {
     
      this.servicejkdRysl()
      this.servicejkdRygc()
      this.servicejkdRygcXq()
    }, 
    getEyesInfo () {
      this.isDialogShow1 = true;
      this.getEyesConten()
    },
     getEyesConten () {
       var params = {
        code: 'zhy-dwjs',
        "area_code":this.cityCode,
      }
      serviceApi.servicejkdTc(params).then(res => {
        
        if (res.rspCode == '0000') {
          var datal = res.result.data[0] || {};
          this.zhy_content = datal.zhy_content
        } else {
          this.$message.error(res.rspMsg)
        }
      })
    },

    teamDetailFn (type, title) {
      var colTeam = []
      this.teamtitle = title
      if (type == 1) {
       colTeam = [
         {'filed': 'name', title: '地市'},
        {'filed': 'nlpy_num', title: '能力培养人数', sortable: true},
         {'filed': 'zw_rz', title: '智网工程师认证人数', sortable: true},
         {'filed': 'rate_rzzw', title: '智网工程师认证比例', sortable: true, rate: true},
         {'filed': 'szh_rz', title: '数字化能力认证人数', sortable: true},
         {'filed': 'rate_rzszh', title: '数字化能力认证比例', sortable: true, rate: true},
       ]
       this.servicejkdRyslXq('servicejkdNlpyXq')
      } else if (type == 2) {
         colTeam = [
         {'filed': 'name', title: '地市'},
         {'filed': 'zydw_num', title: '专业队伍人数', sortable: true},
         {'filed': 'wc_num', title: '创新工程师人数', sortable: true},
         {'filed': 'wc_rate', title: '创新工程师占比', sortable: true, rate: true},
         {'filed': 'wz_num', title: '网络支撑队伍人数', sortable: true},
         {'filed': 'wz_rate', title: '网络支撑队伍占比', sortable: true, rate: true},
         {'filed': 'yw_num', title: '云网工程师人数', sortable: true},
         {'filed': 'yw_rate', title: '云网工程师占比', sortable: true, rate: true},
         {'filed': 'zw_num', title: '智网工程师人数', sortable: true},
         {'filed': 'zw_rate', title: '智网工程师占比', sortable: true, rate: true},
       ]
       this.servicejkdRyslXq('servicejkdZydwXq')
      }  else if (type == 3) {
         colTeam = [
         {'filed': 'name', title: '地市'},
         {'filed': 'zlrc_num', title: '战略人才人数', sortable: true},
         {'filed': 'b_num', title: 'B级人数', sortable: true},
         {'filed': 'c_num', title: 'C级人数', sortable: true},
         {'filed': 'd_num', title: 'D级人数', sortable: true},
        //  {'filed': 'e_num', title: 'E级人数', sortable: true},
        //  {'filed': 'f_num', title: 'F级人数', sortable: true},
        //  {'filed': 'g_num', title: 'G级人数', sortable: true},
       ]
       this.servicejkdRyslXq('servicejkdZlrcXq')
      }
      this.teamCol = colTeam;
      this.isDialogShow = true
    },
    renderRygcChart () {
      var datal = this.cateTeamData
      var chartParams = {
        domId: 'chart_rygc',
        manval: datal.boy,
        womanval:  datal.girl,
        legend: [],
        data: []
      }
      if (this.rowttabStatus == '1') {
        // 年龄
        chartParams.legend = ['35及以下', '35～50', '50以上'],
        chartParams.data = [
           {value: datal.agezhong, name: '35及以下'},
           {value: datal.age35, name: '35～50'},
           {value: datal.age50, name: '50以上'},
        ]
      } else if (this.rowttabStatus == '2') {
          chartParams.legend = ['初中及以下', '高中', '专科', '本科','硕士研究生', '其它'],
          chartParams.data = [
            {value: datal.chuzhong, name: '初中及以下'},
            {value: datal.gaozhong, name: '高中'},
            {value: datal.zhuanke, name: '专科'},
            {value: datal.benke, name: '本科'},
            {value: datal.shuoshi, name: '硕士研究生'},
            {value: datal.qita, name: '其它'},
          ]
      }  else if (this.rowttabStatus == '3') {
          chartParams.legend = ['合同工人数', '外包工人数'],
          chartParams.data = [
            {value: datal.num_ht, name: '合同工人数'},
            {value: datal.num_wb, name: '外包工人数'},
          ]
      }
       echartInit.initPieCat(chartParams)

     
    },
     rowttabsClick (type) {
      this.rowttabStatus = type;
      this.renderRygcChart()
    },
    initSortTabledata (datas) {
      var datal= _.cloneDeep(datas);
       datal.forEach(item => {
        Object.keys(item).forEach(key => {
            Number(item[key]) || Number(item[key]) === 0 ? item[key] =  Number(item[key]) : item[key];
        })
      });
    
      return datal
    }, 
    servicejkdRygc () {
      var params = {
        cityCode: this.cityCode
      }
      serviceApi.servicejkdRygc(params).then((res) => {
        if (res.rspCode == '0000') {
          var datal = res.result.data[0] || {}
          this.cateTeamData = datal;
           this.renderRygcChart()
         } else {
           this.$message.error(res.rspMsg)
         }
      })
    },
     servicejkdRygcXq () {
      var params = {
        cityCode: this.cityCode
      }
      serviceApi.servicejkdRygcXq(params).then((res) => {
        if (res.rspCode == '0000') {
          var datal = res.result.data || []
          this.cateTeamDataDetail = this.initSortTabledata(datal);
         } else {
           this.$message.error(res.rspMsg)
         }
      })
    },
     servicejkdRysl () {
      var params = {
        cityCode: this.cityCode
      }
      serviceApi.servicejkdRysl(params).then((res) => {
        
        if (res.rspCode == '0000') {
          var datal = res.result.data[0] || {}
          this.teamData = datal
         
         } else {
           this.$message.error(res.rspMsg)
         }
      })
    },
    servicejkdRyslXq (service) {
      var params = {
        cityCode: this.cityCode
      }
      this.detailloading = true
      serviceApi[service](params).then((res) => {
         this.detailloading = false
        if (res.rspCode == '0000') {
          var datal = res.result.data || []
          this.teamDataList =  this.initSortTabledata(datal);
         } else {
           this.$message.error(res.rspMsg)
         }
      }).catch(err => {
         this.detailloading = false
      })
    },
   gismapListener(e) {
        var self = this;
        if (e.data.message == 'levelSwitch') {
          self.gisTitle = e.data.area_name ||  this.initgisTitle;
          self.mapLevel = e.data.level
          self.areaCode = e.data.area_id;
          self.gisMapData = e.data;
          self.gisQueryParams = Object.assign({}, self.gisQueryParams, e.data)
          self.gisQueryParams.area_name = '';
          console.log('message', e.data)
          self.cityCode = (e.data.area_id || '370000')
          this.initService()
        }  else if (e.data.message == 'changeCity') {
         
         self.gisTitle = e.data.area_name || '山东省'
         self.mapLevel = e.data.level
         self.areaCode = e.data.area_id;
         self.gisMapData = e.data;
         self.gisQueryParams = Object.assign({},  self.gisQueryParams, e.data)
         self.gisQueryParams.area_name = '';
         self.cityCode = e.data.area_id;
         this.initService()
      } 
      },
      initGisMap(initArea_id) {
        // 初始化地图参数
        this.gisQueryParams = {
          control: this.control.join(','),
          level: this.mapLevel,
          sceneid: '06',
          gis_sceneid: '06',
          area_id: initArea_id || '370000',
          poi_city_id: '',
          system_type: 'C40',
          login_no: store.get('staffId') || '',
