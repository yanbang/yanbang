    initEchart_pie4(params) {
        var echart = echarts.init(document.getElementById(params.domId));
        var rich = {
            aqua: {
                color: "aqua",
                fontSize: 15,
                align: 'left',
                lineHeight: 24
            },
            white: {
                color: "#fff",
                align: 'center',
                fontSize: 16,
            },

            hr: {
                borderColor: '#a2c7f3',
                width: '100%',
                borderWidth: 1,
                height: 0,
                lineHeight: 10
            }
        }
        var img = 'data:image/png;base64,';
        var data = [];
        var color = ['#5092e2', '#6ec940', 'rgb(255, 176, 63)', '#21ffff'];
        for (var i = 0; i < params.data.length; i++) {
            data.push({
                value: params.data[i].value,
                name: params.data[i].name,
                itemStyle: {
                    normal: {
                        borderWidth: 5,
                        shadowBlur: 15,
                        borderColor: color[i],
                        shadowColor: color[i]
                    }
                }
            });
        }
        var seriesOption = [{
            name: '',
            type: 'pie',
            clockWise: false,
            minAngle: 15, //最小角度
            startAngle: 60, //起始角度
            radius: [72, 68],
            hoverAnimation: false,
            itemStyle: {
                normal: {
                    label: {
                        show: true,
                        position: 'outside',
                        margin: 10,
                        color: '#fff',
                        formatter: function name(vparams) {
                            var datal = params.datal.filter(item => {
                                return item.name == vparams.name
                            })[0]
                            return `{white|${datal.name}}\n{hr|}\n{aqua|设备数: ${datal.value}}\n{aqua|端口总数: ${datal.dknum}}\n{aqua|端口占用率: ${datal.ratenum}%}`;
                        },
                        rich: rich
                    },
                    labelLine: {
                        length: 10,
                        length2: 30,
                        show: true,
                        color: '#00ffff'
                    }
                }
            },
            data: data
        }];
        var option = {
            grid: {
                left: "10",
                top: "10",
                right: "10",
                bottom: "10",
                containLabel: true
            },
            color: color,
            title: {
                text: '传输',
                top: '46%',
                textAlign: "center",
                left: "49%",
                textStyle: {
                    color: '#fff',
                    fontSize: 18,
                    fontWeight: '400'
                }
            },
            graphic: {
                elements: [{
                    type: "image",
                    z: 3,
                    style: {
                        image: img,
                        width: 120,
                        height: 120
                    },
                    left: 'center',
                    top: 'center',
                }]
            },
            tooltip: {
                show: false
            },
            series: seriesOption
        }
        echart.setOption(option, true);
    },
    initChartgauge(params) {
        let data = 100;
        var option = {
            title: [{
                text: params.name,
                x: 'center',
                top: '58%',
                textStyle: {
                    color: '#fff',
                    fontSize: 16,
                    fontWeight: '500',
                }
            }, {
                text: params.value + (params.unit || ""),
                x: 'center',
                top: '40%',
                textStyle: {
                    fontSize: 20,
                    color: '#ffcc00',
                    foontWeight: '800',
                },
            }],
            // 极坐标系
            polar: {
                radius: ['60%', '75%'],
                center: ['50%', '50%'],
            },
            // 极坐标系：角度轴
            angleAxis: {
                max: 100 * 360 / 270,
                show: false,
                type: 'value',
                startAngle: 225
            },
            // 极坐标系：径向轴
            radiusAxis: {
                type: 'category',
                show: true,
                axisLabel: {
                    show: false,
                },
                axisLine: {
                    show: false,

                },
                axisTick: {
                    show: false
                },
            },
            series: [
                // 第二层：数据以进度条的形式展示
                {
                    type: 'bar',
                    data: [{
                        value: data
                    }],
                    itemStyle: {
                        color: function() {
                            let obj = {
                                type: 'linear',
                                x: 0, //右
                                y: 0.5, //下
                                x2: 1, //左
                                y2: 0, //上
                                colorStops: [{
                                    offset: 0,
                                    color: '#4978EC'
                                }, {
                                    offset: 1,
                                    color: '#0CD3DB'
                                }]
                            };

                            if (data >= 0 && data < 20) {
                                obj.colorStops[1].color = '#4978EC';
                            } else if (data >= 20 && data < 40) {
                                obj.y = 0.3;
                                obj.x2 = 2;
                            } else if (data >= 40 && data < 60) {
                                obj.y = 0.3;
                                obj.x2 = 1.5;
                            } else if (data >= 60 && data < 80) {
                                obj.y = 0.4;
                                obj.x2 = 1.1;
                            }
                            return obj;
                        }
                    },
                    barGap: '-100%',
                    coordinateSystem: 'polar',
                    roundCap: true,
                    cursor: 'auto',
                    z: 2
                },
                // 第二层：进度条背景
                {
                    type: 'bar',
                    data: [{
                        value: 100,
                    }],
                    itemStyle: {
                        color: '#013f72'
                    },
                    barGap: '-100%',
                    coordinateSystem: 'polar',
                    roundCap: true,
                    cursor: 'auto',
                    emphasis: {
                        itemStyle: {
                            color: '#013f72'
                        }
                    },
                    z: 1
                },
                // 第一层：中心文字展示区
                {
                    type: 'pie',
                    radius: ['0%', '55%'],
                    hoverAnimation: false,
                    center: ['50%', '50%'],
                    cursor: 'auto',
                    itemStyle: {
                        normal: {
                            labelLine: {
                                show: false
                            },
                            color: {
                                type: 'radial',
                                x: 0.5,
                                y: 0.5,
                                r: 0.5,
                                colorStops: [{
                                    offset: 0,
                                    color: '#174869'
                                }, {
                                    offset: 1,
                                    color: '#08385c'
                                }]
                            }
                        }
                    },
                    data: [{
                        value: 100
                    }],
                    z: 2
                },
                // 第三层；仪表盘：只显示刻度
                {
                    type: 'gauge',
                    radius: '82%',
                    startAngle: '225',
                    endAngle: '-45',
                    splitNumber: 10,
                    center: ['50%', '50%'],
                    min: 0,
                    max: 100,
                    pointer: {
                        show: false
                    },
                    title: {
                        show: false
                    },
                    detail: {
                        show: false
                    },
                    data: [{
                        value: 100,
                        name: ''
                    }],
                    axisLine: {
                        lineStyle: {
                            width: 20,
                            color: [
                                [0, '#5fa7ca'],
                                [1, '#5fa7ca']
                            ],
                            opacity: 0
                        }
                    },
                    axisTick: {
                        length: 6,
                        lineStyle: {
                            color: 'auto'
                        }
                    },
                    splitLine: {
                        show: false
                    },
                    axisLabel: {
                        show: false
                    }
                }
            ]
        };
        var echart = echarts.init(document.getElementById(params.domId))
        echart.setOption(option, true);
    },

