import serviceApi from '@/api/wisdomeyes'
var _ = require('@/utils/lodash.js')

var Qs = require('@/utils/qs.js')
export default {
  data() {
    return {
      gisTitle: '山东省',

      isDialogShow: false,
      isDialogShow2: false,
      isDialogShow3: false,
      rowttabStatus: '1',
      rowttabStatus2: '1',
      bzhd_checkList: ['00', '01', '02'],
      cityCode: '370000',
      cityCodeold: '370000',
      iframeSrc: '',
      tabstatus: '0201',
      gisQueryParams: {},
      queryParams: {},
      gisMapData: {},
      mapLevel: 1,
      areaCode: '',
      tabdata: [
        { title: '网络能力', active: false, href: '/unicomwisdomeyes/networkCapab' },
        { title: '健康度', active: false, href: '/unicomwisdomeyes/health' },
        { title: '效能指数', active: false, href: '/unicomwisdomeyes/efficiency_cb' },
        { title: '卓越度', active: false, href: '/unicomwisdomeyes/excellent' },
        { title: '匹配度', active: true, href: '/unicomwisdomeyes/matchin' },
        { title: '队伍建设', active: false, href: '/unicomwisdomeyes/teambuilding' }
      ],
      tabdata1: [
        { title: '存量', active: true, gisid: '0501' },
        { title: '新增', active: false, gisid: '0502' },
        { title: '提速', active: false, gisid: '0503' }
      ],
      control: [],
      sbTabledata: [],
      dwyTabledata: [],
      zhy_content: '',
      matchleftdate: {},
      matchleftdate2: {},
      levelshow: true
    }
  },
  components: {
    Header,
    Footer
  },
  created() {
    self = this
  },
  mounted() {
    var self = this
    var initArea_id = '370000'
    this.mapLevel = 1

    if (store.get('userInfo')) {
      var userInfo = store.get('userInfo')
      var mapLevel = userInfo.levelAreaLevel
      if (mapLevel == '1') {
        self.levelshow = true
        // 全省
        initArea_id = '370000'
        self.gisTitle = '山东省'
        self.mapLevel = '1'
        self.control = ['370000']
      } else if (mapLevel == '2') {
        self.levelshow = true
        // 地市
        initArea_id = '370000'
        self.gisTitle = '山东省'
        self.mapLevel = '1'
        self.control = ['370000', userInfo.levelAreaCode]
      } else if (mapLevel >= '3') {
        self.levelshow = false
        // 区县
        initArea_id = userInfo.levelAreaCode
        self.mapLevel = '2'
        self.gisTitle = userInfo.levelAreaName
        self.control = ['370000', userInfo.levelAreaCode, userInfo.levelCityCode]
      }
    }
    this.cityCode = initArea_id
    this.cityCodeold = initArea_id
    this.initGisMap(initArea_id, '0501')
    // 注册地图事件
    this.initgisTitle = this.gisTitle + ''
    window.addEventListener('message', self.gismapListener, true)
    for (var i = 1; i <= 12; i++) {
      var params1 = {
        domId: 'piechart_' + i,
        value: 0
      }
      new echartInit.initAnipie2(params1)
    }
    this.initService()
  },
  watch: {},
  computed: {
    ...mapState({
      searchDate: state => state.resource.searchDate
    })
  },
  methods: {
    getgis(item) {
      // console.log(item);
      this.tabdata1.forEach(ite => {
        if (ite.title == item.title) {
          ite.active = true
        } else {
          ite.active = false
        }
      })
      this.initGisMap(this.cityCode, item.gisid)
    },
    getEyesInfo() {
      this.isDialogShow3 = true
      this.getEyesConten()
    },
    getEyesConten() {
      var params = {
        code: 'zhy-ppd',
        area_code: this.cityCode
      }
      serviceApi.servicejkdTc(params).then(res => {
        if (res.rspCode == '0000') {
          // //debugger
          var datal = res.result.data[0] || {}
          this.zhy_content = datal.zhy_content
        } else {
          this.$message.error(res.rspMsg)
        }
      })
    },
    jumpage(type) {
      this.getToken(type)
    },
    initService() {
      this.servicejkdSbpp()
      this.servicejkdSbppXq()
      this.servicejkdDwypp()
      this.servicejkdDwyppXq()
    },
    renderChart(domId, val) {
      let params1 = {
        domId: 'piechart_' + domId,
        value: Number(parseFloat(val)) || 0
      }
      new echartInit.initAnipie2(params1)
    },
    servicejkdSbpp() {
      var params = {
        cityCode: this.cityCode
      }
      serviceApi.servicejkdSbpp(params).then(res => {
        if (res.rspCode == '0000') {
          var datal = res.result.data[0] || {}
          this.matchleftdate = datal

          this.renderChart(6, datal.plv_jd)
          // this.renderChart(1, datal.bplv_gm_100)
          // this.renderChart(2, datal.bplv_gm_1000)
          this.renderChart(2, datal.plv_gm)

          // this.renderChart(3, datal.lybplv_gm_100)
          // this.renderChart(4, datal.lybplv_gm_1000)
          this.renderChart(4, datal.plv_ly)

          // this.renderChart(5, datal.linux_rate)
          // this.renderChart(6, datal.android_rate)

          //  this.renderChart(6, datal.plv_jd)  //暂时注释
        } else {
          this.$message.error(res.rspMsg)
        }
      })
    },
    initSortTabledata(datas) {
      var datal = _.cloneDeep(datas)
      datal.forEach(item => {
        Object.keys(item).forEach(key => {
          Number(item[key]) || Number(item[key]) === 0 ? (item[key] = Number(item[key])) : item[key]
        })
      })

      return datal
    },
    servicejkdSbppXq() {
      var params = {
        cityCode: this.cityCode
      }
      serviceApi.servicejkdSbppXq(params).then(res => {
        if (res.rspCode == '0000') {
          var datal = res.result.data || []
          this.sbTabledata = this.initSortTabledata(datal)
        } else {
          this.$message.error(res.rspMsg)
        }
      })
    },

    servicejkdDwyppXq() {
      var params = {
        cityCode: this.cityCode
      }
      serviceApi.servicejkdDwyppXq(params).then(res => {
        if (res.rspCode == '0000') {
          var datal = res.result.data || []
          this.dwyTabledata = this.initSortTabledata(datal)
        } else {
          this.$message.error(res.rspMsg)
        }
      })
    },

    servicejkdDwypp() {
      //写死
      if (this.mapLevel > 4) {
        return
      }
      var params = {
        cityCode: this.cityCode
      }
      serviceApi.servicejkdDwypp(params).then(res => {
        if (res.rspCode == '0000') {
          var datal = res.result.data[0] || {}

          this.renderChart(7, datal.dy_rate_4G)
          this.renderChart(8, datal.dy_rate_5G)

          this.renderChart(9, datal.wy_rate_4G)
          this.renderChart(10, datal.wy_rate_5G)

          this.renderChart(11, datal.dw_rate_4G)
          this.renderChart(12, datal.dw_rate_5G)
        } else {
          this.$message.error(res.rspMsg)
        }
      })
    },

    gismapListener(e) {
      var self = this
      if (e.data.message == 'levelSwitch') {
        self.gisTitle = e.data.area_name || this.initgisTitle
        self.mapLevel = e.data.level
        self.areaCode = e.data.area_id
        self.gisMapData = e.data
        self.gisQueryParams = Object.assign({}, self.gisQueryParams, e.data)
        self.gisQueryParams.area_name = ''
        console.log('message', e.data)
        self.cityCode = e.data.area_id || '370000'
        self.initService()
      }
    },
    initGisMap(initArea_id, gisSceneid) {
      // 初始化地图参数
      this.gisQueryParams = {
        control: this.control.join(','),
        level: this.mapLevel,
        sceneid: '05',
        gis_sceneid: gisSceneid,
        area_id: initArea_id || '370000',
        poi_city_id: '',
        system_type: 'C40',
        login_no: store.get('staffId') || '',
        timest: this.$store.state.resource.searchDate
      }
      var gisQueryParams = JSON.parse(JSON.stringify(this.gisQueryParams))
      this.iframeSrc = window.gis_iframeSrc + `/smartEyes?${Qs.stringify(gisQueryParams)}`
    }
  },
  destroyed() {
    // 在组件生命周期结束的时候销毁。
    window.removeEventListener('message', this.gismapListener, true)
  }
}
</script>

