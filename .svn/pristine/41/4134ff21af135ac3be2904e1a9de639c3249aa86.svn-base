             zoomVal: 100,
            names: ['装机', '移机'],
            lineX: params1_orderData.map(item => {
              return item.name
            }),
            bar_markLine: [this.averageData.kd_xz_rate, this.averageData.kd_yz_rate],
            value: [
                params1_orderData.map(item => {
                  return item.kd_xz_rate
                }),
                params1_orderData.map(item => {
                  return item.kd_yz_rate
                })
            ]

          }
          if (this.mapLevel <= 3) {
              // 写死
             echarts_init.initechart_lines(charts)
          }
         

          var params2_orderData = this.orderDataFn(ftphList, 'kd_fz_rate');
          this.zhyData.avg1 = this.averageData.kd_fz_rate;
          this.zhyData.name1 = params2_orderData[0].name;
          this.zhyData.value1 = params2_orderData[0].kd_fz_rate;
          let charts2 = {
            domId: 'chart_line2',
            unit: '百分比',
             zoomVal: 100,
            names: ['宽带发展', '固话发展', '移网发展'],
            lineX: params2_orderData.map(item => {
              return item.name
            }),
            value: [
               params2_orderData.map(item => {
                  return item.kd_fz_rate
                }),
                params2_orderData.map(item => {
                  return item.gh_fz_rate
                }),
                params2_orderData.map(item => {
                  return item.yw_fz_rate
                }),
            ],
           bar_markLine: [this.averageData.kd_fz_rate, this.averageData.gh_fz_rate, this.averageData.yw_fz_rate],

          }

           if (this.mapLevel <= 3) {
             // 写死
             echarts_init.initechart_lines(charts2);
          }
         
         
      },
      

      getList() {
         var cityCode = this.cityCode
         var qparams =  {
           cityCode:  this.cityCode,
           or_he: this.or_he,
         }
         if (this.or_he == '0') {
           serviceKdZhXnYh (qparams).then(res => {
           if (res.rspCode == '0000') {
              var responseData = res.result.data;
              var datal = responseData.filter(item => {
                return item.code != '0000'
              })
              var avdatal = responseData.filter(item => {
                return item.code == '0000'
              })
              this.ftphList = datal;
              this.averageData = avdatal.length > 0 ? avdatal[0] : {}
              this.render_wcxnTable()
              this.render_ztzjTable()
              this.render_csjsTable()
              this.render_ztgdTable()
              this.fillParam()
             } else {
              this.$message.error(res.rspMsg)
            }
          })
         } else {
            efficacyList2(qparams).then(res => {
           if (res.rspCode == '0000') {
              var responseData = res.result.data;
              var datal = responseData.filter(item => {
                return item.code != '0000'
              })
              var avdatal = responseData.filter(item => {
                return item.code == '0000'
              })
              this.ftphList = datal;
              this.averageData = avdatal.length > 0 ? avdatal[0] : {}
              this.render_wcxnTable()
              this.render_ztzjTable()
              this.render_csjsTable()
              this.render_ztgdTable()
              this.fillParam()
             } else {
              this.$message.error(res.rspMsg)
            }
          })
      
         }
        
      },

     

      wcxntabsClick(type) {
        this.wcxnStatus = type
      },
      rowttabsClick(type) {
        this.rowttabStatus = type;
        this.fillParam('change_status')
      },
      gismapListener(e) {
        var self = this;
        if (e.data.message == 'levelSwitch') {
          self.gisTitle = e.data.area_name || this.initgisTitle;
          self.mapLevel = e.data.level
          self.areaCode = e.data.area_id;
          self.gisMapData = e.data;
          self.gisQueryParams = Object.assign({}, self.gisQueryParams, e.data)
          self.gisQueryParams.area_name = '';
          console.log('message', e.data)
          self.cityCode = e.data.area_id

          if (e.data.level == 2) {
            self.leve2CityCode =  e.data.area_id;
          } else if (e.data.level == 1) {
            self.leve2CityCode =  ''
          }
          this.getList()
        }
      },
      initGisMap(initArea_id) {
        // 初始化地图参数
        this.gisQueryParams = {
           yanhuang: this.or_he,
           control: this.control.join(','),
          level: this.mapLevel,
          sceneid: 3,
          gis_sceneid: this.tabstatus,
          area_id: initArea_id,
          poi_city_id: '',
          system_type: 'C40',
          login_no: store.get('staffId') || '',
          timest: this.$store.state.resource.searchDate
        };
        var gisQueryParams = JSON.parse(JSON.stringify(this.gisQueryParams))
        this.iframeSrc = window.gis_iframeSrc +  `/smartEyes?${Qs.stringify(gisQueryParams)}`;

      },

      tabStatusFn(type) {
        var gisQueryParams = JSON.parse(JSON.stringify(this.gisQueryParams));
        gisQueryParams.message = "changeType";
        gisQueryParams.gis_sceneid = type;
        this.$refs['gisiframe'].contentWindow.postMessage(gisQueryParams, '*')
      },
      initEchart() {

        var charts = {
          domId: 'chart_line1',
          unit: '百分比',
          names: ['装机', '移机'],
          lineX: ['济南1', '济南2', '济南3', '济南4', '济南5', '济南6', '济南7', '济南8'],
          value: [
            [41, 32, 33, 34, 95, 36, 17, 28],
            [36, 45, 80, 12, 30, 50, 92, 80]
          ]

        }
        echarts_init.initechart_lines(charts)


        var charts2 = {
          domId: 'chart_line2',
          unit: '百分比',
          names: ['宽带发展', '固话发展', '移网发展'],
          lineX: ['济南1', '济南2', '济南3', '济南4', '济南5', '济南6', '济南7', '济南8'],
          value: [
            [41, 32, 33, 34, 95, 36, 17, 28],
            [36, 45, 80, 12, 30, 50, 92, 80],
            [16, 55, 20, 78, 10, 60, 82, 10]
          ]

        }
        echarts_init.initechart_lines(charts2)

        var charts2 = {
          domId: 'chart_line3',
          unit: '百分比',
          names: ['公客履约率', '联系客户及时率', '催单率', '7天重复障碍率'],
          lineX: ['济南1', '济南2', '济南3', '济南4', '济南5', '济南6', '济南7', '济南8'],
          value: [
            [41, 32, 33, 34, 95, 36, 17, 28],
            [36, 45, 80, 12, 30, 50, 92, 80],
            [16, 55, 20, 78, 10, 60, 82, 10],
            [16, 25, 20, 52, 70, 10, 22, 60],
          ]

        }
        echarts_init.initechart_lines(charts2)


      }
    },
    destroyed() { // 在组件生命周期结束的时候销毁。
      window.removeEventListener('message', this.gismapListener, true)
    }
  }
</script>

