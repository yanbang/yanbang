        if (res.rspCode == '0000') {
            var datal = res.result.data || [];
            this.rightData4G = datal.filter(item => {
              return item.type == '4G'
            })
            this.rightData5G = datal.filter(item => {
              return item.type == '5G'
            })
           　this.initechart()
        } else {
          this.$message.error(res.rspMsg)
        }
      })
   },
   serviceYeSdtHulianwang  () {
     var params = {
        cityCode: this.cityCode,
      }
      serviceApi.serviceYeSdtHulianwang(params).then((res) => {
        

        if (res.rspCode == '0000') {
             var datal = res.result.data || [];
             this.listData = datal;
        } else {
          this.$message.error(res.rspMsg)
        }
      })
   },
   serviceYeSdtHealthy  () {
     var params = {
        cityCode: this.cityCode,
      }
      serviceApi.serviceYeSdtHealthy(params).then((res) => {
        
        if (res.rspCode == '0000') {
            var datal = res.result.data || [];
            var edata = datal.reverse().map(item => {
              return {name: item.type, value: Number(item.healthy) || 0}
            })
             var chartparams = {
                domId: 'sdbarbox',
                data: edata
            }
            new echartInit.initdcbar(chartparams)
        } else {
          this.$message.error(res.rspMsg)
        }
      })
   },
   serviceYeSdtZxsy () {
     var params = {
        cityCode: this.cityCode,
      }
      serviceApi.serviceYeSdtZxsy(params).then((res) => {
        if (res.rspCode == '0000') {
             this.sbt_jkdata = res.result.data[0] || {}
        } else {
          this.$message.error(res.rspMsg)
        }
      })
   },
    rowttabsClick (type) {
      this.rowttabStatus = type
      this.initechart()
    },
    initechart () {
      var rightData4G = this.rightData4G;
      var rightData5G = this.rightData5G;

    var ratekey = ''
    if (this.rowttabStatus == '1') {
        ratekey = 'prb_rate'
    } else if (this.rowttabStatus == '2') {
        ratekey = 'avg_rate'
    }
      var linechart1 = {
        domId: 'rightchartbox1',
        xAxis: rightData4G.map(item => {
          return item.in_time
        }),
        data4g:rightData4G.map(item => {
          return item[ratekey]
        }),
        data5g: rightData5G.map(item => {
          return  item[ratekey]
        }),
      }
       new echartInit.initline2(linechart1)

         var sumtotal = [];
         for (let i = 0; i < rightData4G.length; i++ ) {
           let sumval = (Number(rightData4G[i].avg_user) || 0) + (Number(rightData5G[i].avg_user) || 0)
           sumtotal.push(sumval)
         }
         var echartParams = {
            domId: 'stackBar1',
            rotate: 30,
            sumtotal: sumtotal,
            xData: rightData4G.map(item => {
              return item.in_time
            }),
            legend:  ['4G', '5G'],
            data: [
              rightData4G.map(item => {
                return item['avg_user']
              }),
              rightData5G.map(item => {
                return  item['avg_user']
              }),
            ]
        }
          new echartInit.initEchart_sckbar(echartParams)

      
     },
      initGisMap(initArea_id) {
      // 初始化地图参数
        this.gisQueryParams = {
            control: this.control.join(','),
            level:self.mapLevel,
            sceneid: '8',
            gis_sceneid: '',
            area_id:  '0531',
            poi_city_id: '',
            system_type: 'C40',
            login_no: store.get('staffId') || '',
            timest: this.$store.state.resource.searchDate
        }
        var gisQueryParams = JSON.parse(JSON.stringify(this.gisQueryParams))
        this.iframeSrc = window.gis_iframeSrc + `/smartEyes?${Qs.stringify(gisQueryParams)}`;
    }
  },
  destroyed () {
    window.clearInterval(jktimer)
  }
    
}

