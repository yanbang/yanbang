     if (store.get('userInfo')) {
        var userInfo =  store.get('userInfo')
        var mapLevel = userInfo.levelAreaLevel;
        // 分权分域
        if (mapLevel == '1') {
          // 全省
          initArea_id = '370000'
          self.gisTitle = '山东省'
          self.mapLevel = '1'
          self.control = ['370000']
        } else if (mapLevel == '2') {
          // 地市
          initArea_id = '370000'
          self.gisTitle = '山东省'
          self.mapLevel = '1'
          self.control = ['370000', userInfo.levelAreaCode]
        } else if (mapLevel >= '3') {
          // 区县
          initArea_id = userInfo.levelAreaCode
          self.mapLevel = '2'
          self.gisTitle = userInfo.levelAreaName
          self.control = ['370000', userInfo.levelAreaCode, userInfo.levelCityCode]
        } 
      }
     this.cityCode = initArea_id;
     this.initGisMap(initArea_id);
     // 注册地图事件
     this.initgisTitle = this.gisTitle + '';
      window.addEventListener('message',self.gismapListener, true)

    this.initService()
  },
  watch: {},
  computed: {
    ...mapState({
       searchDate: state => state.resource.searchDate,
    }),
  },
  methods: {
    initService () {
      this.zqZhyx()
      this.zqYwnl()
      this.zqApp()
    },
    formatNumber (num) {
      if (Number(num)) {
        return Number(num).toLocaleString()
      } else {
        return ''
      }
    },
    zqZhyx () {
      var params = {
        cityCode: this.cityCode
      }
      serviceApi.zqZhyx(params).then((res) => {
         if (res.rspCode == '0000' && res.result.data) {
            var bdata = res.result.data[0] || {};
       
            bdata.rack_cnt_pre = bdata.idc_use_rack_cnt && bdata.idc_total_rack_cnt ?  ((bdata.idc_use_rack_cnt/bdata.idc_total_rack_cnt) * 100).toFixed(2) : '';
            bdata.brand_cn_pre =  Number(bdata.idc_max_branduse_rate || 0).toFixed(2)
            bdata.por_tcnt_pre = bdata.idc_spare_por_tcnt && bdata.idc_serviceport_cnt? ((bdata.idc_spare_por_tcnt/bdata.idc_serviceport_cnt) * 100).toFixed(2) : ''
            this.leftData = res.result.data[0] || {};
              var params = {
                domId: 'anpieChart',
                value: ((bdata.hlw_spare_port_cnt/bdata.hlw_total_port_cnt) * 100).toFixed(2),
              }
            echarts_zq.initAnipie2(params)
              this.otnData = bdata
            
         } else {
           this.$message.error(res.rspMsg)
         }
      })
    },

     zqYwnl () {
      var params = {
        cityCode: this.cityCode
      }
      serviceApi.zqYwnl(params).then((res) => {
          if (res.rspCode == '0000' && res.result.data) {
             this.rightData = res.result.data[0] || {};
             this.initRightData()
          }
      }).catch((err) => {
        
      });
    },
    initRightData () {
      var rowttabStatus = this.rowttabStatus;
      var rightData = this.rightData

      var params2 = {
            domId: 'pieChartgu1',
            name:'校园总数',
            value: ''
      }
      var params22 = {
          domId: 'pieChartgu2',
          name:'端业不匹配率',
          unit: '%',
          value: ''
        }
      

      var wdata = {
        id: 'waterPoloChart1',
        value: '',
      }

      var wdata2 = {
        id: 'waterPoloChart2',
        value: '',
      }

      if (rowttabStatus == 1) {
        params2.name = '校园总数';
        params2.value = rightData.school_cnt || '';
        params22.value = rightData.school_porservno_rate || '';
        wdata.value = rightData.school_4gflow_ratio || ''
        wdata2.value = rightData.school_5gflow_ratio || ''

        this.G45data.G4_fg = rightData.school_4gstation_cnt || '';
        this.G45data.G4_user = rightData.school_4guser_cnt || '';
        this.G45data.G5_fg = rightData.school_5gstation_cnt || '';
        this.G45data.G5_user = rightData.school_5guser_cnt || ''

      } else {
        params2.name = '商务楼宇总数';
        params2.value = rightData.louyu_cnt || '';
        params22.value = rightData.louyu_porservno_rate || '';
         wdata.value = rightData.louyu_4gflow_ratio || ''
        wdata2.value = rightData.louyu_5gflow_ratio || ''

        this.G45data.G4_fg = rightData.louyu_4gstation_cnt || ''
        this.G45data.G4_user = rightData.louyu_4guser_cnt || '';
        this.G45data.G5_fg = rightData.louyu_5gstation_cnt || '';
        this.G45data.G5_user = rightData.louyu_5guser_cnt || ''
      }

      echarts_zq.initChartgauge(params2)
      echarts_zq.initChartgauge(params22)
      waterPoloChart.initChart(wdata)
      waterPoloChart.initChart(wdata2)
    },
     zqApp () {
      var params = {
        cityCode: this.cityCode
      }
      serviceApi.zqApp(params).then((res) => {
         if (res.rspCode == '0000') {
            var datal = res.result.data;
            this.appsortData = datal || []


            var datal_xy = _.sortBy(datal, function(item){
              return  Number(item.app_sort);
            });
            var datal_ly = _.sortBy(datal, function(item){
              return  Number(item.app_sort_ly);
            });

            this.zhyData.xydata.name = datal_xy[0].app_name
            this.zhyData.xydata.value = datal_xy[0].app_flow
            
            this.zhyData.lydata.name = datal_ly[0].app_name
            this.zhyData.lydata.value = datal_ly[0].app_flow_ly

            this.renderChart_APP()
            
         } else {
           this.$message.error(res.rspMsg)
         }
      })
    },
    renderChart_APP () {
      
      var qparams1 = {
        domId: 'chart_appfbll',
        data: []
      }
      var rowttabStatus = this.rowttabStatus;
      var datal = this.appsortData;
      if (rowttabStatus == 1) {
        datal = _.sortBy(datal, function(item){
          return  Number(item.app_sort);
        });
        
        qparams1.data = datal.map(item => {
          return {'name': item.app_name, val: item.app_flow}
        })
      } else {
        datal = _.sortBy(datal, function(item){
          return  Number(item.app_sort_ly || 0);
        });
        
        qparams1.data = datal.map(item => {
          return {'name': item.app_name_ly || '-', val: item.app_flow_ly}
        })
      }
      
      
      echarts_zq.initEchart_pm(qparams1)
    },
    rowttabsClick (type) {
      this.rowttabStatus = type;
      this.initRightData()
      this.renderChart_APP()
    },
    gismapListener (e) {
      var self = this;
      if (e.data.message == 'levelSwitch') {
         self.gisTitle =  e.data.area_name || this.initgisTitle;
         self.mapLevel = e.data.level
         self.areaCode = e.data.area_id;
         self.gisMapData = e.data;
         self.gisQueryParams = Object.assign({},  self.gisQueryParams, e.data)
         self.gisQueryParams.area_name = '';
         console.log('message', e.data)
         self.cityCode = e.data.area_id;
         self.initService();
      } else if (e.data.message == 'clickBuild') {
         this.buildiframeshow = true;
         this.buildiframessrc = e.data.buildUrl
      }
    },
     initGisMap (initArea_id) {
      // 初始化地图参数
          this.gisQueryParams =  {
             control: this.control.join(','),
             level: this.mapLevel,
              sceneid: 2,
              gis_sceneid: this.tabstatus,
              area_id: initArea_id,
              poi_city_id: '',
              system_type: 'C40',
              login_no:  store.get('staffId') || '',
              timest:  this.$store.state.resource.searchDate
          };
          var gisQueryParams = JSON.parse(JSON.stringify(this.gisQueryParams))
          this.iframeSrc = window.gis_iframeSrc + `/smartEyes?${Qs.stringify(gisQueryParams)}`;
          
    },
    tabStatusFn (type) {
      this.tabstatus = type;
      var gisQueryParams = JSON.parse(JSON.stringify(this.gisQueryParams));
      gisQueryParams.message = "changeType";
      gisQueryParams.gis_sceneid = type;
      this.$refs['gisiframe'].contentWindow.postMessage(gisQueryParams, '*')
    },
    initEchart () {
        var params2 = {
        domId: 'pieChartgu1',
        name:'校园总数',
        value: '34346'
      }
       echarts_zq.initChartgauge(params2)


      var params22 = {
          domId: 'pieChartgu2',
          name:'端业不匹配数量',
          value: '34346'
        }
       echarts_zq.initChartgauge(params22)

      let data = {
        id: 'waterPoloChart1',
        value: 80,
      }
      waterPoloChart.initChart(data)


      let data2 = {
        id: 'waterPoloChart2',
        value: 30,
      }
      waterPoloChart.initChart(data2)

      var qparams1 = {
        domId: 'chart_appfbll',
        uni: '(次)',
        data: [
          {'name': '基站车', val: 1000},
          {'name': '电源车', val: 983},
          {'name': '短波车', val: 823},
          {'name': '保障车', val: 734},
          {'name': '通勤车', val: 653},
          {'name': '集群车', val: 612},
          {'name': '卫星车', val: 567},
          {'name': '动中通', val: 432},
        ]
      }
      echarts_zq.initEchart_pm(qparams1)
      
    },
    
