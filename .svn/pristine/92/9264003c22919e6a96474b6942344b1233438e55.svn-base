<script>
var self = null
import Header from '@/components/resource_visual/header2'
import Footer from '@/components/resource_visual/footer'
import store from 'store'
import { mapState } from 'vuex'
import echartInit from './initechart'
import serviceApi from '@/api/wisdomeyes'
var _ = require('@/utils/lodash.js')

var Qs = require('@/utils/qs.js')
export default {
  data() {
    return {
      gisTitle: '山东省',

      isDialogShow: false,
       isDialogShow2: false,

      rowttabStatus: '1',
      rowttabStatus2: '1',
      bzhd_checkList: ['00', '01', '02'],
      cityCode: '370000',
      iframeSrc: '',
      tabstatus: '0201',
      gisQueryParams: {},
      queryParams: {},
      gisMapData: {},
      mapLevel: 1,
      areaCode: '',
      tabdata: [
        { title: '网络能力', active: false, href: '/unicomwisdomeyes/networkCapab' },
        { title: '健康度', active: false, href: '/unicomwisdomeyes/health' },
        { title: '效能指数', active: true, href: '/unicomwisdomeyes/efficiency_sr' },
        { title: '卓越度', active: false, href: '/unicomwisdomeyes/excellent' },
        { title: '匹配度', active: false, href: '/unicomwisdomeyes/matchin' },
        { title: '队伍建设', active: false, href: '/unicomwisdomeyes/teambuilding' }
      ],
      tabdata2: [
         { title: '成本效能', active: false, href: '/unicomwisdomeyes/efficiency_cb' },
         { title: '收入效能', active: true, href: '/unicomwisdomeyes/efficiency_sr' }, 
      ],
      control:[],
      dxqData: {},
      detaliTitle: '',
      xnsrTableData: [],
      tableCol: [],
      tableloading: false,
      zhy_content: ''

    }
  },
  components: {
    Header,
    Footer
  },
  created() {
    self = this
  },
  mounted() {
    var self = this
    var initArea_id = '370000'
    this.mapLevel = 1
   if (store.get('userInfo')) {
      var userInfo = store.get('userInfo')
      var mapLevel = userInfo.levelAreaLevel
      if (mapLevel == '1') {
          // 省级用户
          this.control = ['370000']
        } else if (mapLevel >= '2') {
          this.control = ['370000', userInfo.levelAreaCode]
        }
    }
    this.cityCode = initArea_id
    this.initGisMap(initArea_id)
    // 注册地图事件
    this.initgisTitle = this.gisTitle + ''
    window.addEventListener('message', self.gismapListener, true)
    this.initService()
  },
  watch: {},
  computed: {
    ...mapState({
      searchDate: state => state.resource.searchDate
    })
  },
  methods: {
    initService() {
      this.servicejkdDdk();
      this.servicejkdDou()
      this.servicejkdMou() 
    },
    getEyesInfo () {
      this.isDialogShow = true;
      this.getEyesConten()
    },
     getEyesConten () {
       var params = {
        code: 'zhy-xnzscb'
      }
      serviceApi.servicejkdTc(params).then(res => {
        
        if (res.rspCode == '0000') {
          var datal = res.result.data[0] || {};
          this.zhy_content = datal.zhy_content
        } else {
          this.$message.error(res.rspMsg)
        }
      })
    },
    
    xnsrDetailFn (title, type) {
      
      this.detaliTitle = title;
      this.xnsrTableData = []
      if (type == '1') {
        this.tableCol = [
          {filed: 'array_dxq', title: '单小区收入排名', width: 160},
          {filed: 'name', title: '省份'},
          {filed: 'encome_yw', title: '移网收入(万元)', sortable: true},
          {filed: 'num_xq', title: '345g小区数', sortable: true},
          {filed: 'encome_dxq', title: '单小区收入(万元)', sortable: true},
        ]
         this.servicejkdDdkXq(1)
      } else if  (type == '2') {
        this.tableCol = [
          {filed: 'array_dp', title: '单端口收入排名',  width: 160},
          {filed: 'name', title: '省份'},
          {filed: 'encome_kd', title: '宽带网收入(万元)', sortable: true},
          {filed: 'num_port', title: '占用端口数', sortable: true},
          {filed: 'encome_dp', title: '单端口收入(元)', sortable: true},
        ]
         this.servicejkdDdkXq(2)
      } else if (type == '3') {
        this.tableCol = [
          {filed: 'name', title: '省份'},
          // {filed: 'arpu', title: 'arpu(元)', sortable: true},
          {filed: 'dou', title: 'dou(MB)', sortable: true},
        ]
         this.servicejkdMouDou()
      }
      else if (type == '4') {
        this.tableCol = [
          {filed: 'name', title: '省份'},
          // {filed: 'arpu', title: 'arpu(元)', sortable: true},
          {filed: 'mou', title: 'mou(分钟)', sortable: true},
        ]
         this.servicejkdMouDou()
      }
      this.isDialogShow2 = true
    },
    servicejkdMou  () {
      var params = {
        cityCode: this.cityCode
      }
      serviceApi.servicejkdMou(params).then(res => {
        if (res.rspCode == '0000') {
          var datal = res.result.data || []
          let chartparams = {
                domId: 'chartbox2',
                yname: "分钟",
                xAxis: datal.map(item => {
                  return item.timest
                }),
                datas: [
                    {
                        name: 'VOLTE用户',
                        data: datal.map(item => {
                          return item.num_vo || 0
                        }),
                        type: 'bar',
                        barWidth: 15,
                    },
          
                    {
                        name: '3G用户',
                        data:  datal.map(item => {
                          return item.num_3g || 0
                        }),
                        type: 'bar',
                        barWidth: 15,
                   },
                             {
                        name: '2G用户',
                        data:    datal.map(item => {
                          return item.num_2g || 0
                        }),
                        type: 'bar',
                       barWidth: 15,
                    },
                ]
            }
            echartInit.initEchart_mbar(chartparams)

        } else {
          this.$message.error(res.rspMsg)
        }
      })
    },
    servicejkdDou () {
      var params = {
        cityCode: this.cityCode
      }
      serviceApi.servicejkdDou(params).then(res => {
        if (res.rspCode == '0000') {
          var datal = res.result.data || []
          let chartparams = {
                domId: 'chartbox1',
                yname: "MB",
                xAxis: datal.map(item => {
                  return item.timest
                }),
                datas: [
                    {
                        name: '5GSA用户',
                        data: datal.map(item => {
                          return item.num_5g || 0
                        }),
                        type: 'bar',
                        barWidth: 15,
                    },
                    {
                        name: '4G用户',
                        data:    datal.map(item => {
                          return item.num_4g || 0
                        }),
                        type: 'bar',
                       barWidth: 15,
                    },
                    {
                        name: '3G用户',
                        data:  datal.map(item => {
                          return item.num_3g || 0
                        }),
                        type: 'bar',
                        barWidth: 15,
                   }
                ]
            }
            echartInit.initEchart_mbar(chartparams)

        } else {
          this.$message.error(res.rspMsg)
        }
      })
    },
    servicejkdDdk () {
      var params = {
        cityCode: this.cityCode
      }
      serviceApi.servicejkdDdk(params).then(res => {
        
        if (res.rspCode == '0000') {
          
            var datal = res.result.data[0] || {}
            this.dxqData = datal

        } else {
          this.$message.error(res.rspMsg)
        }
      })
    },
    servicejkdMouDou (type) {
      var params = {
        cityCode: this.cityCode
      }
      this.tableloading = true
      serviceApi.servicejkdMouDou(params).then(res => {
        this.tableloading = false
        if (res.rspCode == '0000') {
            var datal = res.result.data || []
           
            this.xnsrTableData = this.initSortTabledata(datal);

        } else {
          this.$message.error(res.rspMsg)
        }
      })
    },
   orderDataFn (datas, key) {
       datas.forEach(item => {
         item.orderIndex = item[key]
       })
        return  _.orderBy(datas, function (item) {
            return   Number(item[key]) || Number(item[key]) == 0 ? Number(item[key]) : '-';
        }, ['asc']);
    },
    servicejkdDdkXq (type) {
      var params = {
        cityCode: this.cityCode
      }
      this.tableloading = true
      serviceApi.servicejkdDdkXq(params).then(res => {
        this.tableloading = false
        if (res.rspCode == '0000') {
            var datal = res.result.data || []
            if (type == '1') {
                datal = this.orderDataFn(datal, 'array_dxq');
            } else if (type == '1') {
                datal = this.orderDataFn(datal, 'array_dp');
            }
            
            this.xnsrTableData = this.initSortTabledata(datal);

        } else {
          this.$message.error(res.rspMsg)
        }
      })
    },
    initSortTabledata (datas) {
      var datal= _.cloneDeep(datas);
       datal.forEach(item => {
        Object.keys(item).forEach(key => {
            Number(item[key]) || Number(item[key]) === 0 ? item[key] =  Number(item[key]) : item[key];
        })
      });
    
      return datal
    }, 

    gismapListener(e) {
      var self = this
      if (e.data.message == 'levelSwitch') {
        self.gisTitle = e.data.area_name || this.initgisTitle
        self.mapLevel = e.data.level
        self.areaCode = e.data.area_id
        self.gisMapData = e.data
        self.gisQueryParams = Object.assign({}, self.gisQueryParams, e.data)
        self.gisQueryParams.area_name = ''
        console.log('message', e.data)
        self.cityCode = e.data.area_id || '370000'
        this.initService()
      }  else if (e.data.message == 'changeCity') {
         return
         self.gisTitle = e.data.area_name || '山东省'
         self.mapLevel = e.data.level
         self.areaCode = e.data.area_id;
         self.gisMapData = e.data;
         self.gisQueryParams = Object.assign({},  self.gisQueryParams, e.data)
         self.gisQueryParams.area_name = '';
         self.cityCode = e.data.area_id;
         this.initService()
      } 
    },
    initGisMap(initArea_id) {
      // 初始化地图参数
      this.gisQueryParams = {
        control: this.control.join(','),
        level: this.mapLevel,
        sceneid: '03',
        gis_sceneid: '0302',
        area_id: initArea_id || '370000',
        poi_city_id: '',
        system_type: 'C40',
        login_no: store.get('staffId') || '',
        timest: this.$store.state.resource.searchDate
      }
      var gisQueryParams = JSON.parse(JSON.stringify(this.gisQueryParams))
      this.iframeSrc = window.gis_iframeSrc + `/smartEyes?${Qs.stringify(gisQueryParams)}`
    }
  },
  destroyed() {
    // 在组件生命周期结束的时候销毁。
    window.removeEventListener('message', this.gismapListener, true)
  }
}
</script>

