 .el-dialog {
  -webkit-transform: none;
  transform: none;
  left: 0;
  position: relative;
  margin: 0 auto;
  height: 780px;
  background-image: url('~@/assets/images/bigScreen/kuang.png');
  background-size: 100% 100%;
  background-color: rgba(220, 38, 38, 0);

}
.v-modal{
    z-index: -1 !important;
}
 .el-dialog__body {
  padding: 0px 10px;
}
 .el-dialog__headerbtn .el-dialog__close {
  color: #fff;
  font-size: 24px;
  font-weight: 500;
  margin: 2px 20px 0 0;
  border: 2px solid #193987;
  border-radius: 5px;
  background: #1e1e88;
}

.el-table {
  background: none !important;
  font-size: 14px;
}
.el-table th {
  background: none !important;
  background: url('~@/assets/images/bigScreen/fullService/597bcdc505fa3.png') no-repeat !important;
  background-position: center 11px !important;
  background-size: contain !important;
  text-align: center !important;
  color: #02f6ff !important;
}

 .dialogTable .el-table th {
  background: none !important;
  background-position: center 11px !important;
  background-size: contain !important;
  text-align: center !important;
  color: #02f6ff !important;
  font-size: 10px;
}
 .el-table tr {
  background: none !important;
}

.el-table td {
  background: none !important;
  border: none !important;
  text-align: center !important;
  color: #ffffff;
}
.dialogTableBg .el-table td {
  background: none !important;
  border: none !important;
  text-align: center !important;
  color: #ffffff;
  font-family: DigifaceWide !important;
  padding: 6px 0;
}
.dialogTableBg td {
  background: url('~@/assets/images/bigScreen/fullService/Shape 1387.png') no-repeat left !important;
}
.el-table td:first-child {
  background: none !important;
}
.dialogTableBg .el-table td:first-child {
  font-family: Microsoft YaHei !important;
}
 .el-table th.is-leaf,
.el-table td {
  border: none !important;
  color: #ffffff;
}
.el-table::before {
  height: 0 !important;
}
 .el-table th > .cell {
  display: contents !important;
}
.el-table .cell {
  padding: 0;
}
.el-table .ascending .sort-caret.ascending {
  border-bottom-color: #02f6ff;
}

 .el-table .descending .sort-caret.descending {
  border-top-color: #02f6ff;
}
.bsdialog .el-dialog {
  -webkit-transform: none;
  transform: none;
  left: 0;
  position: relative;
  margin: 0 auto;
  background-image: url("~@/assets/images/bigScreen/fullService/fixDataBg.png");
  background-size: 100% 100%;
  background-color:rgba(220,38,38,0);

}
.bsdialog  .el-dialog__header {
  padding: 30px 10px 10px 40px;
}
.bsdialog  .el-dialog__title {
  font-size: 18px;
  color: #fff;
}
.bsdialog .el-dialog__headerbtn{
  position: absolute;
  top: 24px;
  right: 50px;
  padding: 0;
  background: transparent;
  border: none;
  outline: none;
  cursor: pointer;
  font-size: 20px;
}
.bsdialog .el-dialog__headerbtn .el-dialog__close{
  color: #fff !important;
  margin:0 -20px 0 0 !important;
}


.bsdialog  .el-dialog__body {
  padding: 10px 50px;
  color: #fff;
  font-size: 14px;
  word-break: break-all;
  padding-bottom: 40px;

}
.tagtabs {
