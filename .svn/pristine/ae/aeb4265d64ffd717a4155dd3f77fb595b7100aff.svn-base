     getEyesConten () {//地市智慧眼
       this.isDialogShow_ZHY = true;
       var params = {
        code:'fu-zwkdfn',
        "area_code": this.cityCode,
      };
      this.queryEyesInfo(params).then(res => {
        this.zhy_content = res
      })
      
    },

    initSortTabledata (datas, nokey) {
      var datal= _.cloneDeep(datas);
       datal.forEach(item => {
       
        Object.keys(item).forEach(key => {
            if (key != nokey) {
              Number(item[key]) || Number(item[key]) === 0 ? item[key] =  Number(item[key]) : item[key];
            }
            
        })
      });
      return datal
    },

      formatNumber (num) {
        if (Number(num) || Number(num) == 0) {
          return Number(num).toLocaleString()
        } else {
          return ''
        }
      },


      gdmxDetailFn2 (row, key) {
        this.serviceKdGdmx2(row, key)
      },

      serviceKdGdmx2 (row, key) {
         var busiTypeMap = {
           '020101': '1',
           '020102': '2',
           '020201': '3',
           '020202': '4',
           '020301': '5',
           '020302': '6',
           '020401': '7',
           '020402': '8'
         }
          var busiTypeMap2 = {
           '020101': '装机预警',
           '020102': '装机超时',
           '020201': '修机预警',
           '020202': '修机超时',
           '020301': '投诉预警',
           '020302': '投诉超时',
           '020401': '群障预警',
           '020402': '群障超时'
         }
         this.gdxq_title = row.name + '-' + busiTypeMap2[key] + '清单';
         var params = {
            busiType: busiTypeMap[key],
            cityCode: row.code
          }
          this.pm_tableDataDetail = []
          this.tdloading = true
          this.gdxq_isDialogShow = true
          serviceKdGdmx(params).then((res) => {
            this.tdloading = false
            var datal = res.result.data || []
            this.pm_tableDataDetail = datal           
          }).catch(err => {
             this.tdloading = false
          })
      },
        
      getList(){
        var self = this;
        var cityCode = this.cityCode
        this.ftphList = []
         this.gdDetailmodelTable = []
         var qparams = {
           cityCode: this.cityCode,
           or_he: this.or_he,
         }
         if (this.or_he  == '0') {
           this.serviceKdYh()
         } else {
           warnList2(qparams).then(response => {
            var datal = response.result.data.filter(item => {
              return item.code !=  '0000'
            });
            this.ftphList = datal;
            this.gdDetailmodelTable = this.initSortTabledata(datal, 'code')
          })
         }
        
      },
      serviceKdYh () {
       let params={
          cityCode:this.cityCode,
          or_he: this.or_he
        }
        newkdfnService['serviceKdYh'] (params).then(res => {
           this.comeloading  =  false;
          if (res.rspCode == '0000') {
            
            var datal =  res.result.data.filter(item => {
              return item.code !=  '0000'
            }) || [];
            

            this.ftphList = datal;
            this.gdDetailmodelTable = this.initSortTabledata(datal, 'code')
            
          } else {
            this.$message.error(res.rspMsg)
          }
        })
    },

    gismapListener (e) {
      if (e.data.message == 'levelSwitch') {
        
         self.gisTitle = e.data.area_name ||  this.initgisTitle
         self.mapLevel = e.data.level
         self.areaCode = e.data.area_id;
         self.gisMapData = e.data;
         self.gisQueryParams = Object.assign({},  self.gisQueryParams, e.data)
         self.gisQueryParams.area_name = '';
         console.log('message', e.data)
         self.cityCode = e.data.area_id;
         self.initService()
         self.getList()
      }
    },
    initGisMap (initArea_id) {
       // 初始化地图参数
        this.gisQueryParams = {
          yanhuang: this.or_he,
          level: this.mapLevel,
          control: this.control.join(','),
          sceneid: 3,
          gis_sceneid: this.gisStatus,
          area_id: initArea_id,
          poi_city_id: '',
          system_type: 'C40',
          login_no: store.get('staffId') || '',
          timest: this.$store.state.resource.searchDate
        };
        var gisQueryParams = JSON.parse(JSON.stringify(this.gisQueryParams))

        this.iframeSrc = window.gis_iframeSrc + `/smartEyes?${Qs.stringify(gisQueryParams)}`;
    },

 
  },
  updated() {
     var self = this;
  },
  destroyed() { // 在组件生命周期结束的时候销毁。
    window.removeEventListener('message',this.gismapListener, true)
  }
}
