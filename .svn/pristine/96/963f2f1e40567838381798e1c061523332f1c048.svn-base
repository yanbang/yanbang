      myChart.on('click', (param) => {
        // 权限控制区
        // let isLimit = that.limitList.some((item, i) => {
        //   return item === param.name
        // })
        // if (!isLimit) {
        //   alert('对不起，无权查看！')
        //   return
        // }
        console.log(param)
        if (param.name in provinces || param.name in cityMap || param.name === 'china') {
          that.mapStack.push({
            name: that.curMap.name
          })
        }
        if (param.name in provinces) {
          // 处理省模块
          let names = param.name
          for (let key in provinces) {
            if (names === key) {
              showProvince(provinces[key], key)
              break
            }
          }
        } else if (param.name in cityMap) {
          // 处理市模块
          let names = param.name
          for (let key in cityMap) {
            if (names === key) {
              showCitys(cityMap[key], key)
              break
            }
          }
        }
      })

      /**
           绑定右键事件
        */
      myChart.on('contextmenu', function(params) {
        console.log(params.event.offsetX)
        console.log(params.event.offsetY)
        params.event.event.preventDefault()
        $('#contextMenu')
          .css({
            left: params.event.offsetX,
            top: params.event.offsetY
          })
          .show()
      })

      /**
           响应图表的右键事件，返回上一级地图
        */
      $('#contextMenu').on('click', function() {
        $(this).hide()
        // 获取上一级地图信息
        var map = that.mapStack.pop()
        if (!map) {
          alert('已经到达最上一级地图了')
          return
        } else {
          if (map.name in provinces) {
            // 处理省模块
            let names = map.name
            for (let key in provinces) {
              if (names === key) {
                console.log('进入省份逻辑： ' + names)
                showProvince(provinces[key], key)
                break
              }
            }
          } else if (map.name in cityMap) {
            // 处理市模块
            let names = map.name
            for (let key in cityMap) {
              if (names === key) {
                console.log('进入市逻辑： ' + names)
                showCitys(cityMap[key], key)
                break
              }
            }
          } else if (map.name === 'china') {
            initEcharts(map.name)
          }
        }
      })
      // 待优化
      function showProvince(eName, param) {
        require('../../../../static/mapjs/' + param)
        initEcharts(param)
      }
      // 待优化
      function showCitys(cName, param) {
        require('../../../../static/mapjs/' + param)
        initEcharts(param)
      }
