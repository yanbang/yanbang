  methods: {
     getSummaries(param) {
        
        const { columns, data } = param;
        const sums = [];
        columns.forEach((column, index) => {
          if (index === 0) {
            sums[index] = '合计';
            return;
          } else if (index == 3) {
            sums[3] = ((sums[2] / sums[1]) * 100).toFixed(2) + '%'
          } else {
              const values = data.map(item => Number(item[column.property]));
              values.forEach(val => {
                if ( isNaN(val)) {
                  val = 0;
                }
              })
              sums[index] = values.reduce((prev, curr) => {
                const value = Number(curr);
                if (!isNaN(value)) {
                  return prev + curr;
                } else {
                  return prev;
                }
              }, 0);
          }  
        });

        return sums;
    },
    getSummaries2 (param) {
        const { columns, data } = param;
        const sums = ['平均值', this.zyyjData.addres_match || 0,  this.zyyjData.addres_out || 0, this.zyyjData.wlt_cell_cnt || 0, this.zyyjData.wlt_baoqu_cnt || 0 , this.zyyjData.wlt_ftth_cnt || 0];
        return sums;
