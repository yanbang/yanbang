import request from '@/utils/request'

const api = {
    serviceFuTss: '/ZhyFuCon/serviceFuMyd', // 投申诉
    serviceFuFgq: 'ZhyFuCon/serviceFuFgq' // 分光器全匹配散点
}

var serviceApi = {


    serviceFuTss(parameter) {
        return request({
            url: api.serviceFuTss,
            method: 'post',
            data: parameter
        })
    },
    serviceFuFgq(parameter) {
        return request({
            url: api.serviceFuFgq,
            method: 'post',
            data: parameter
        })
    },


}

export default serviceApi