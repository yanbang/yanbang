              break;
          default:
              rotename = ''
              ratename = ''
              rate = true
      } 
 
      var tableCol = [
        {filed: 'name', title: '地市'},
        // {filed: val + '_northsort', title: '北十排名',  sortable: true},
        // {filed: val + '_sort', title: '全国排名', sortable: true},
         {filed: ratename, title: rotename,  sortable: true, rate: rate},
