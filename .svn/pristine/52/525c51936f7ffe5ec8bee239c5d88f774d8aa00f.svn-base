@import url('./comme.scss');
.stitle {
  line-height: 50px;
  text-align: left;
  font-size: 20px;
  margin-left: 36px;
  color: #02f6ff;
}
.tabsbox {
  display: flex;
  justify-content: space-between;
  margin-top: 5px;
  font-size: 16px;
}
.tabs-item {
  position: relative;
  width: 48%;
  height: 22px;
  line-height: 22px;
  text-align: center;
}
.tab_borderleft {
  contain: '';
  height: 22px;
  width: 15px;
  position: absolute;
  border: 1px solid #4f98f7;
  left: 0;
  top: 0;
  border-right: none;
}
.tab_borderright {
  contain: '';
  height: 22px;
  width: 15px;
  position: absolute;
  border: 1px solid #4f98f7;
  right: 0;
  top: 0;
  border-left: none;
}
.tabfon {
  color: #4f98f7;
  cursor: pointer;
  font-size: 16px;
}
.tabs-item.active .tab_borderleft {
  border: 1px solid #02f6ff;
  border-right: none;
}
.tabs-item.active .tab_borderright {
  border: 1px solid #02f6ff;
  border-left: none;
}
.tabs-item.active .tabfon {
  color: #02f6ff;
}
.tabs-item.active {
  background: radial-gradient(#4df0f775, #144e9836);
}
.tjbox {
  background: #0b2a56;
  margin-top: 6px;
  padding: 4px 8px;
}
.tjitem {
  flex: 1;
}
.tjbox .tjitem:last-child {
  flex: 1.2;
}
.tjnf {
  font-size: 14px;
  color: #02f6ff;
  margin-right: 10px;
}
.echart_linebox {
  height: 50%;
  position: relative;
}
.echarttile {
  position: absolute;
  left: 0;
  top: 10px;
  z-index: 10;
}
.echarttile .tabfon {
  color: #02f6ff;
}
.tjitem .numfont {
  font-size: 16px;
}
.lefttbox {
  display: flex;
  flex-direction: column;
}
.flex1 {
  flex: 1;
}
.ltbomf {
  text-align: center;
}
.ltbomf dt {
  font-size: 14px;
}
.ltbomf dd {
  color: #02f6ff;
  margin-bottom: 2px;
}
.ltbomf::after {
  content: '';
  height: 6px;
  width: 100%;
  background: url('~@/assets/images/bigScreen/ligtdian.png') no-repeat;
  background-position: center;
  display: block;
}
.liquidbox {
  display: flex;
  flex-direction: column;
}
.liquiditem {
  flex: 1;
  display: flex;
}
.liqleft {
  display: flex;
  align-items: center;
  position: relative;
}
.liqleft::after {
  content: '';
  display: block;
  background: url('~@/assets/images/bigScreen/resource/split.png') no-repeat;
  background-size: 100% 100%;
  height: 70%;
  width: 1px;
  margin-left: 10px;
}
.liqright {
  flex: 1;
  margin-left: 10px;
  display: flex;
}
.liqechartbox {
  flex: 1;
  display: flex;
  flex-direction: column;
  text-align: center;
}
.liqechartbox .numfont {
  color: #02f6ff;
}
.liqechartbox .nfont {
  font-size: 13px;
}
.statename {
  width: 100%;
  display: flex;
  margin-bottom: 15px;
}
.statename_right {
  flex: 1;
  margin-left: 10px;
  color: #02f6ff;
  display: flex;
  flex-direction: column;
}
.rstitle {
  height: 28px;
  line-height: 28px;
  font-size: 16px;
}
.rborderc {
  height: 24px;
  line-height: 24px;
  width: 60%;
  background: url('~@/assets/images/bigScreen/resource/rborder.png') no-repeat;
  background-size: 100% 100%;
  text-align: center;
  font-size: 13px;
}
.rborderc .numfont {
  color: #fff;
  margin-left: 10px;
}
.stateinfo {
  padding: 5px;
  margin-top: 0px;
  background: #0a274e;
  display: flex;
  font-size: 14px;
  line-height: 26px;
  border-radius: 5px;
}
.stateitmleft {
  position: relative;
}
.stateitmleft::after {
  content: '';
  position: absolute;
  right: 5px;
  top: 0;
  display: block;
  background: url('~@/assets/images/bigScreen/resource/split.png') no-repeat;
  background-size: 100% 100%;
  height: 100%;
  width: 1px;
}
.ybpchartbox {
  display: flex;
  flex-direction: column;
  margin-top: 10px;
  height: 100px;
}
.echartGaugebox {
  position: relative;
  flex-direction: column;
  text-align: center;
}
.echartGaugebox .nfont {
  position: absolute;
  bottom: 0px;
  text-align: center;
  width: 100%;
}
.peiitem {
  position: relative;
}
.peiitem .piefon {
  position: absolute;
  bottom: -14px;
  text-align: center;
  width: 100%;
  font-size: 13px;
}
.rbtbox {
  width: 100%;
  height: 24px;
  line-height: 24px;
}
.stabitem {
  padding: 2px 4px;
  border: 1px solid #4f98f7;
  color: #4f98f7;
  border-radius: 4px;
  font-size: 14px;
  cursor: pointer;
}
.stabitem:not(:last-child) {
  margin-right: 10px;
}
.stabitem.active {
  border: 1px solid #02f6ff;
  color: #02f6ff;
}

.imgrybox {
  width: auto;
  margin: 0 auto;
  display: flex;
  padding: 5px 20px;
}
.tagitem {
  display: inline-block;
  margin-right: 6px;
  font-size: 14px;
  padding: 5px 8px;
  background: url('~@/assets/images/bigScreen/resource/tag2.png') no-repeat;
  background-size: 100% 100%;
  color: #218cc8;
  min-width: 60px;
  text-align: center;
  cursor: pointer;
}
.tagitem.active {
  background: url('~@/assets/images/bigScreen/resource/tag1.png') no-repeat;
  background-size: 100% 100%;
  color: #02f6ff;
}
.statety {
  width: 100%;
  height: 140px;
  background: url('~@/assets/images/bigScreen/fullService/left-top.png') no-repeat;
  background-size: 100% 100%;
  display: flex;
}
.statety .flex1 {
  padding-top: 48px;
  text-align: center;
  font-size: 16px;
}
.flex1center {
  position: relative;
  margin: 5px;
  box-sizing: border-box;
  background: url('~@/assets/images/bigScreen/fullService/left-center.png') no-repeat;
  background-size: 100% 100%;
}
.ybpchartbox {
  position: relative;
}
.piexxq {
  position: absolute;
  right: 0px;
  cursor: pointer;
  top: 0px;
  font-size: 14px;
}
.subdtile {
  font-size: 16px;
  float: right;
  color: #a5e8fe;
}
.tagtabs {
  margin-bottom: 15px;
}
.tagtabs .el-tag {
  line-height: 24px;
  height: 26px;
  font-size: 14px;
  background-color: transparent;
  border-color: #a3d3ff;
  color: #acceef;
  margin-right: 10px;
  cursor: pointer;
}
.el-tag.selected {
  background: #3c73c5;
  color: #fff;
}
.morediv {
  cursor: pointer;
}
.flextab2 {
  display: flex;
  flex-direction: column;
}
.relative {
  position: relative;
}
.nfonttt {
  position: absolute;
  bottom: 0px;
  text-align: center;
  width: 100%;
}
.lev4tab2n {
  text-align: center;
  margin-top: 30px;
  width: 100px;
  font-size: 16px;
  position: relative;
}
.moret2 {
  position: absolute;
  bottom: 10px;
  right: 10px;
  color: #02f6ff;
  cursor: pointer;
}
.tabsbox2 {
  position: relative;
}



.rstitle {
  height: 32px; 
  line-height: 32px;
  font-size: 16px;
  background: url("~@/assets/images/bigScreen/fullService/biaoti.png") no-repeat right;
  margin-bottom: 5px;
  
}
.rborderc {
  height: 24px;
  line-height: 24px;
  width: 50%;
  background: url("~@/assets/images/bigScreen/resource/rborder.png") no-repeat;
  background-size: 100% 100%;
  text-align: center;
  font-size: 14px;
}