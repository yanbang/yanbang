      //不能删
      orderDataFn(datas, key) {
        return _.orderBy(
          datas,
          function(item) {
            return Number(item[key]) || 0
          },
          ['desc']
        )
      },


      wcxntabsClick(type) {
        this.wcxnStatus = type
      },
      rowttabsClick(type) {
        this.rowttabStatus = type
