            })
            var avrData3 =
              response.filter(item => {
                return item.code == self.cityCode
              })[0] || {}
           
            var orderData3 = this.orderDataFn(datal3, 'login_rate')
            var orderData_lldk = this.orderDataFn(datal3, 'no_port_nums')

            var chartParams6 = {
              domId: 'chart_line6',
              legend: ['5G登网率'],
               zoomVal: 100,
              name: '5G登网率',
              unit: '数量',
              color: 'rgba(237,161,54, 0.8)',
              // bar_markLine: Number(avrData3.no_port_nums),
              xData: orderData3.map(item => {
                return item.name
              }),
              barData: orderData3.map(item => {
                return item.login_rate
              }),
              // line_markLine: Number(avrData3.no_fgq_nums) || 0,
              // lineData: orderData3.map(item => {
              //   return item.login_rate
              // })
            }
            echarts_init.initEchart_barLine(chartParams6)
            // SA登网率
            var datal4 = response.filter(item => {
              return item.code != self.cityCode
            })
            var avrData4 =
              response.filter(item => {
                return item.code == self.cityCode
              })[0] || {}
            
            var orderData4 = this.orderDataFn(datal4, 'sa_login_rate')
            var orderData_lldk = this.orderDataFn(datal4, 'no_port_nums')

            var chartParams8 = {
               zoomVal: 100,
              domId: 'chart_line8',
              legend: ['SA登网率'],
              name: 'SA登网率',
              unit: '数量',
              color: 'rgba(237,161,54, 0.8)',
              bar_markLine: Number(avrData4.no_port_nums),
