              // lineData: orderData4.map(item => {
              //   return item.prb_4g
              // })
            }
            echarts_init.initEchart_barLine(chartParams8)
            // 5G终端覆盖率
            var datal9 = response.filter(item => {
              return item.code !=  self.cityCode
            })
            var avrData9 =
              response.filter(item => {
                return item.code ==  self.cityCode
              })[0] || {}
            
            var orderData9 = this.orderDataFn(datal9, 'wg_fugai_rate')
            var orderData_lldk = this.orderDataFn(datal9, 'no_port_nums')

            var chartParams9 = {
               zoomVal: 100,
              domId: 'chart_line9',
              legend: ['5G终端覆盖率'],
