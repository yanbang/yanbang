    <style>
        .first-loading-wrp {
            display: flex;
            justify-content: center;
            align-items: center;
            flex-direction: column;
            min-height: 420px;
            height: 100%
        }
        
        .first-loading-wrp>h1 {
            font-size: 128px
        }
        
        .first-loading-wrp .loading-wrp {
            padding: 98px;
            display: flex;
            justify-content: center;
            align-items: center
        }
        
        .dot {
            animation: antRotate 1.2s infinite linear;
            transform: rotate(45deg);
            position: relative;
            display: inline-block;
            font-size: 32px;
            width: 32px;
            height: 32px;
            box-sizing: border-box
        }
        
        .dot i {
            width: 14px;
            height: 14px;
            position: absolute;
            display: block;
            background-color: #1890ff;
            border-radius: 100%;
            transform: scale(.75);
            transform-origin: 50% 50%;
            opacity: .3;
            animation: antSpinMove 1s infinite linear alternate
        }
        
        .dot i:nth-child(1) {
            top: 0;
            left: 0
        }
        
        .dot i:nth-child(2) {
            top: 0;
            right: 0;
            -webkit-animation-delay: .4s;
            animation-delay: .4s
        }
        
        .dot i:nth-child(3) {
            right: 0;
            bottom: 0;
            -webkit-animation-delay: .8s;
            animation-delay: .8s
        }
        
        .dot i:nth-child(4) {
            bottom: 0;
            left: 0;
            -webkit-animation-delay: 1.2s;
            animation-delay: 1.2s
        }
        
        @keyframes antRotate {
            to {
                -webkit-transform: rotate(405deg);
                transform: rotate(405deg)
            }
        }
        
        @-webkit-keyframes antRotate {
            to {
                -webkit-transform: rotate(405deg);
                transform: rotate(405deg)
            }
        }
        
        @keyframes antSpinMove {
            to {
                opacity: 1
            }
        }
        
        @-webkit-keyframes antSpinMove {
            to {
                opacity: 1
            }
        }
        
        .el-picker-panel {
            border: 1px solid #00FFFF !important;
            background: #030d46 !important;
            color: #6a79ce !important;
            height: 250px !important;
            width: 355px !important;
            overflow: hidden !important;
        }
        
        .el-date-picker__header {
            margin: 12px 12px 0 12px !important;
        }
        
        .el-date-picker__header-label {
            color: #00f9fa !important;
        }
        
        .el-date-table th {
            color: #6a79ce !important;
            border: none !important;
            text-align: center !important;
            width: 46px !important;
            padding: 0 !important;
        }
        
        .el-date-table td {
            width: 46px !important;
            height: 25px !important;
            padding: 0 !important;
        }
        
        .el-month-table td .cell {
            color: #6a79ce !important;
        }
        /* .el-month-table td.current:not(.disabled) .cell {
      color: #1890ff !important;
    } */
        
        .el-date-table td.disabled div {
            background: none !important;
        }
        
        .el-date-table td span {
            margin: 5px auto !important;
            border: 0.5px solid #00FFFF;
            color: #FFFFFF;
            background: radial-gradient(circle, #19206E 67%, #04DAF8 100%) !important;
            border-radius: 50%;
            height: 20px !important;
            width: 20px !important;
            font-size: 14px !important;
            line-height: 20px !important;
        }
        
        .el-date-table td div {
            height: 19px !important;
            padding: 0 !important;
        }
        
        .el-date-table__row .disabled div span {
            border: none !important;
            color: #6082E3 !important;
            background: none !important;
        }
        
        .el-date-table__row .current div span {
            background: #6082E3 !important;
            color: #ffffff !important;
            border: none !important;
            height: 24px !important;
            width: 24px !important;
            line-height: 24px !important;
        }
        
        .trend_div .main canvas {
            height: 600px !important;
        }
        
        .trend_div_Width .main canvas {
            height: 280px !important;
        }
        
        .bottom div {
            /* background-image: url('../../assets/images/bigScreen/btn_tab.png') !important;
      background-repeat: no-repeat;
      background-size: 100% 100%;
      height: 61px; */
        }
        
        .dialogTableBg1 .el-table th td:last-child {
            border: none !important;
        }
        
        .myloading .el-icon-loading {
            color: rgb(153, 149, 149) !important;
            font-size: 60px !important;
        }
        
        .myloading .el-loading-spinner .el-loading-text {
            color: #ddd !important;
            font-size: 22px !important;
        }
