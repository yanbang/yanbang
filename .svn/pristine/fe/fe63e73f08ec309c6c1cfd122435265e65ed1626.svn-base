<script>
var self = null
import Header from './Header';
import Header2 from '@/components/resource_visual/header';
import Footer from '@/components/resource_visual/footer';
import store from 'store'
import { mapState } from 'vuex'
import echartInit from './wxfn_echart'
import serviceApi from './serviceApi'
var _ = require('@/utils/lodash.js')
var Qs = require('@/utils/qs.js')
export default {
  data () {
    return {
      gisTitle: '山东省',
      isDialogShow: false,
      rowttabStatus: '1',
      rowttabStatus2: '1',
      bzhd_checkList: ['00', '01', '02'],
      cityCode: '370000',
      iframeSrc: '',
      tabstatus: '0201',
      gisQueryParams: {},
      queryParams: {},
      gisMapData: {},
      mapLevel: 1,
      areaCode: '',
      wxfnDatas: {},
      tabdata: [
        // {title: '应急通信', active: false, href: '/resource_visual/cris_communication'},
        {title: '无线赋能', active: true, href: '/resource_visual/jsc_wxfn'},
      ],
      control: [],
      initgisTitle: '',
    }
  },
  components: {
    Header,
    Footer,
    Header2
   
  },
  created() {
    self = this;
  },
  mounted () {
     var initArea_id = '370000'
     this.mapLevel = 1;
      if (store.get('userInfo')) {
          var userInfo =  store.get('userInfo')
           var mapLevel = userInfo.levelAreaLevel;
          if (mapLevel == '1') {
            // 全省
            initArea_id = '370000'
            self.gisTitle  = '山东省'
            self.mapLevel = '1';
            self.control = []
          } else if (mapLevel == '2') {
            // 地市
            initArea_id = '370000'
            self.gisTitle  = '山东省'
            self.mapLevel = '1';
            self.control = ['370000', userInfo.levelAreaCode]
          } else if (mapLevel == '3') {
            // 区县
            initArea_id = userInfo.levelAreaCode;
            self.mapLevel = '2';
            self.gisTitle  = userInfo.levelAreaName
            self.control = ['370000', userInfo.levelAreaCode, userInfo.levelCityCode]
          }  else if (mapLevel >= '4') {
            // 营服
            initArea_id = userInfo.levelCityCode;
            self.gisTitle  = userInfo.levelCityName
            self.mapLevel = '3';
            self.control = ['370000', userInfo.levelAreaCode, userInfo.levelCityCode, userInfo.levelYfCode]
        }
      }
    this.cityCode = initArea_id;
    this.initGisMap(initArea_id);
    this.initgisTitle = self.gisTitle + '';
     // 注册地图事件
    window.addEventListener('message',self.gismapListener, true);
    this.initService()
    // this.initEchart()
  },
  watch: {
    
  },
  computed: {
    ...mapState({
       searchDate: state => state.resource.searchDate,
    }),
   mapLevel_Area () {
     var aname = ''
     var mapLevel = this.mapLevel;
     if (mapLevel == 1) {
       aname = '地市'
     } else if (mapLevel == 2) {
        aname = '区县'
     } else if (mapLevel == 3) {
        aname = '营服'
     }
     return aname
   },
   mapLevel_pname () {
     var aname = ''
     var mapLevel = this.mapLevel;
     if (mapLevel == 1) {
       aname = '全省'
     } else if (mapLevel == 2) {
        aname = '全市'
     } else if (mapLevel == 3) {
        aname = '全区县'
     }
     return aname
   },
    
  },
  methods: {

    rowttabsClick (type) {
      this.rowttabStatus = type;
      
    },

    initService () {
      this.serviceWxApp()
      this.serviceWxWxzb()
    },
    serviceWxApp () {
      var params = {
         "cityCode": this.cityCode
      };
      serviceApi.serviceWxApp(params).then(res => {
        if (res.rspCode == '0000') {
           this.renderApp_pm(res.result.data)
         } else {
           this.$message.error(res.rspMsg)
        }
      })
    },
    serviceWxWxzb () {
      var params = {
         "cityCode": this.cityCode
      };
      serviceApi.serviceWxWxzb(params).then(res => {
        if (res.rspCode == '0000') {
           this.wxfnDatas = res.result.data[0]
           this.renderEcharts()
           this.$nextTick(function (params) {
            //  self.isDialogShow = true
           })
         } else {
           this.$message.error(res.rspMsg)
        }
      })
    },
     
   
    gismapListener (e) {
      if (e.data.message == 'levelSwitch') {
         self.gisTitle = e.data.area_name ||  this.initgisTitle
         self.mapLevel = e.data.level
         self.areaCode = e.data.area_id;
         self.gisMapData = e.data;
         self.gisQueryParams = Object.assign({},  self.gisQueryParams, e.data)
         self.gisQueryParams.area_name = '';
         console.log('message', e.data)
         self.cityCode = (e.data.area_id || '370000');
         self.initService()
      } 
    },
    initGisMap (initArea_id) {
      // 初始化地图参数
        this.gisQueryParams =  {
              control: this.control.join(','),
              level: this.mapLevel,
              sceneid: 4,
              gis_sceneid: '0101',
              area_id: initArea_id,
              poi_city_id: '',
              system_type: 'C40',
              login_no:  store.get('staffId') || '',
              timest:  this.$store.state.resource.searchDate
          };
          var gisQueryParams = JSON.parse(JSON.stringify(this.gisQueryParams))
      
        this.iframeSrc = window.gis_iframeSrc + `/smartEyes?${Qs.stringify(gisQueryParams)}`;
          
    },
    tabStatusFn (type) {
      this.tabstatus = type;
      var gisQueryParams = JSON.parse(JSON.stringify(this.gisQueryParams));
      gisQueryParams.message = "changeType";
      gisQueryParams.gis_sceneid = type;
      this.$refs['gisiframe'].contentWindow.postMessage(gisQueryParams, '*')
    },
    renderApp_pm (data) {
      var datal = _.sortBy(data, function (item) {
        return Number(item.app_sort)
      });
      var appdata = datal.map(item => {
        return  {'name': item.app_name, 'val': item.app_flow};
      })
      var chartparams2 = {
        domId: 'chart_appph',
        uni: '',
        data: appdata
      }
      new echartInit.initEchart_pm(chartparams2)
    },
    renderEcharts () {
      var datas = this.wxfnDatas;
      var pieparams4 = {
          domId: 'compareBar1',
          Xdata: ['3G', '4G', '5G'],
          data: [
            [datas.user_3g_nums, datas.user_4g_nums, datas.user_5g_nums],
            [datas.physics_station_3g_nums, datas.physics_station_4g_nums, datas.physics_station_5g_nums],
          ],
        }
       new echartInit.initBar_muchart(pieparams4);
        var chartparams3 = {
          domId: 'gaugechart1',
          val: parseInt(datas.avg_4g_download_rate * 100/100),
          
        }
        new echartInit.initecahrt_gauge(chartparams3)

        var chartparams32 = {
          domId: 'gaugechart2',
          val: parseInt(datas.avg_4g_upload_rate * 100/10) ,
        }
        new echartInit.initecahrt_gauge(chartparams32)




    },
    initEchart () {
        var pieparams4 = {
          domId: 'compareBar1',
          Xdata: ['3G', '4G', '5G'],
          data: [
            [345, 434, 567],
            [1345, 2434, 3567],
          ],
        }
        new echartInit.initBar_muchart(pieparams4)

        // var pieparams42 = {
        //   domId: 'compareBar2',
        //   data: [6345, 2434, 4567],
        // }
        // new echartInit.initCompareBar(pieparams42)


        // var chartparams2 = {
        //   domId: 'chart_appph',
        //   uni: 'G',
        //   data: [
        //     {'name': '抖音', val: 90},
        //     {'name': '微信', val: 83},
        //     {'name': '快手', val: 73},
        //     {'name': 'QQ', val: 73},
        //     {'name': '今日头条', val: 65},
        //     {'name': '爱奇艺', val: 61},
        //     {'name': '腾讯视屏', val: 56},
        //     {'name': '新浪微博', val: 43},
        //     {'name': '新浪微博2', val: 43},
        //     {'name': '新浪微博3', val: 43},
        //   ]
        // }
        // new echartInit.initEchart_pm(chartparams2)

         var chartparams3 = {
            domId: 'gaugechart1',
         }
         new echartInit.initecahrt_gauge(chartparams3)

         var chartparams32 = {
            domId: 'gaugechart2',
         }
         new echartInit.initecahrt_gauge(chartparams32)
        
   }
  },
  updated() {
     var self = this;
  },
  destroyed() { // 在组件生命周期结束的时候销毁。
    window.removeEventListener('message',this.gismapListener, true)
  }
}
</script>

