import request from '@/utils/request';

const api = {
    serviceYeSdtZxsy: '/ZhYeCon/serviceYeSdtZxsy', //5.5.2.1	实时在线用户数和时延
    serviceYeSdtHealthy: '/ZhYeCon/serviceYeSdtHealthy', //5.5.2.1	健康度
    serviceYeSdtHulianwang: '/ZhYeCon/serviceYeSdtHulianwang', //互联网专线
    serviceYeSdtAvgzhibiao: '/ZhYeCon/serviceYeSdtAvgzhibiao', //PRB平均利用率、平均用户数、下行平均速率专线
    serviceYeSdtJifanggisf: '/ZhYeCon/serviceYeSdtJifanggisf', //中间地图机房撒点数据
    serviceYeSdtJizhandetail: '/ZhYeCon/serviceYeSdtJizhandetail', //机房下基站清单
}

var serviceApi = {}
Object.keys(api).forEach(key => {
    serviceApi[key] = function(parameter) {

        return request({
            url: api[key],
            method: 'post',
            data: parameter
        })
    }
})

export default serviceApi