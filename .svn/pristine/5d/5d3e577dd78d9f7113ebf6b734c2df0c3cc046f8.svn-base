      //   self.mapLevel = '2'
      //   self.gisTitle = userInfo.levelAreaName
      //   self.control = ['370000', userInfo.levelAreaCode, userInfo.levelCityCode]
      // }
    }
    this.cityCode = initArea_id
    this.initGisMap(initArea_id)
    this.initgisTitle = this.gisTitle + ''

    // 注册地图事件
    window.addEventListener('message', self.gismapListener, true)
    this.initService()
  },
  watch: {},
  computed: {
    ...mapState({
      searchDate: state => state.resource.searchDate
    })
  },
  methods: {
    initService() {
      this.serviceItyWlnl()
      this.serviceItyResource()
      this.serviceItyEquipuserate()
      this.serviceItyGuzhangnum()
      
    },
     getEyesConten () {//地市智慧眼
       this.isDialogShow_ZHY = true;
       var params = {
        code:'yun-it',
        "area_code": '370000',
      };
      this.queryEyesInfo(params).then(res => {
        this.zhy_content = res
      })
      
    },
    zycDetailFn () {
        this.isDialogShow_ZYC = true;
    },

    serviceItyWlnl() {
      let params = {
        cityCode: this.cityCode
      }
      serviceApi.serviceItyWlnl(params).then(res => {
        if (res.rspCode == '0000') {
          var datal = res.result.data[0] || {}
          this.wlnlData = datal
        } else {
          this.$message.error(res.rspMsg)
        }
      })
    },
    serviceItyResource() {
      let params = {
        cityCode: this.cityCode
      }
      serviceApi.serviceItyResource(params).then(res => {
        if (res.rspCode == '0000') {
          var datal = res.result.data || []
          this.zycData = datal
        } else {
          this.$message.error(res.rspMsg)
        }
      })
    },
    serviceItyEquipuserate () {
      let params = {
        cityCode: this.cityCode
      }
      
      serviceApi.serviceItyEquipuserate(params).then(res => {
        if (res.rspCode == '0000') {
          var datal = res.result.data || []
          this.mslylTable = datal
        } else {
          this.$message.error(res.rspMsg)
        }
      })
    },
    serviceItyGuzhangnum () {
      let params = {
        cityCode: this.cityCode
      }
      
      serviceApi.serviceItyGuzhangnum(params).then(res => {
        if (res.rspCode == '0000') {
          var datal = res.result.data || []
          var legend =  [
              {'name': '通用云', key: 'yun_ty'},
              {'name': '桌面云', key: 'yun_zm'},
              {'name': '大数据', key: 'big_data'},
              {'name': 'OSS云', key: 'yun_oss'}
          ]
           var echartParams = {
                domId: 'stackBar1',
                rotate: 45,
                sumtotal:  datal.map(item => {
                    return item.total
                }),
                xData: datal.map(item => {
                    return item.yeartime
                }),
                legend:  legend.map(item => {
                    return item.name
                }),
                data: legend.map(item => {
                  return  datal.map(sitem => {
                        return ( Number(sitem[item.key]) || 0)
                    })
                }) 
            }
             new echartInit.initEchart_sckbar(echartParams)
        } else {
          this.$message.error(res.rspMsg)
        }
      })
    },
    rowttabsClick(type) {
      this.rowttabStatus = type
    },
    gismapListener(e) {
      if (e.data.message == 'levelSwitch') {
        ;(self.gisTitle = e.data.area_name || '山东省'), (self.mapLevel = e.data.level)
        self.areaCode = e.data.area_id
        self.gisMapData = e.data
        self.gisQueryParams = Object.assign({}, self.gisQueryParams, e.data)
        self.gisQueryParams.area_name = ''
        console.log('message', e.data)
        self.cityCode = e.data.area_id || '370000'
      }
    },

    initGisMap(initArea_id) {
      // 初始化地图参数
      this.gisQueryParams = {
        level: '2' || this.mapLevel,
        sceneid: '015',
        gis_sceneid: '015',
        area_id: '0531' || initArea_id,
        poi_city_id: '',
        system_type: 'C40',
        login_no: store.get('staffId') || '',
        timest: this.$store.state.resource.searchDate
      }
      var gisQueryParams = JSON.parse(JSON.stringify(this.gisQueryParams))
      this.iframeSrc = window.gis_iframeSrc + `/smartEyes?${Qs.stringify(gisQueryParams)}`
    },

    initEchart() {
      // var echartParams2 = {
      //   domId: 'chartGauge1',
      //   value: 40,
      //   name: 'CPU'
      // }
      // new echartInit.initEcahrt_Sgauge(echartParams2)

      //  var echartParams3 = {
      //   domId: 'chartGauge2',
      //   value: 15,
      //   name: '内存'
      // }
      // new echartInit.initEcahrt_Sgauge(echartParams3)

      //  var echartParams22 = {
      //   domId: 'chartGauge3',
      //   value: 40,
      //   name: '储存(分配率)'
      // }
      // new echartInit.initEcahrt_Sgauge(echartParams22)

      //  var echartParams33 = {
      //   domId: 'chartGauge4',
      //   value: 15,
      //   name: '网路端口利用率'
      // }
      // new echartInit.initEcahrt_Sgauge(echartParams33)

     
    }
  },
  destroyed() {
    // 在组件生命周期结束的时候销毁。
    window.removeEventListener('message', this.gismapListener, true)
  }
}
