  components: {
    Header,
    Footer
  },
  created() {
    self = this
  },
  mounted() {
    var initArea_id = '370000'
    this.mapLevel = 1
    if (store.get('userInfo')) {
      var userInfo = store.get('userInfo')
      var mapLevel = userInfo.levelAreaLevel
      initArea_id = '370000'
      self.gisTitle = '山东省'
      self.mapLevel = '1'
      self.control = []
      // if (mapLevel == '1') {
      //   // 全省
      //   initArea_id = '370000'
      //   self.gisTitle = '山东省'
