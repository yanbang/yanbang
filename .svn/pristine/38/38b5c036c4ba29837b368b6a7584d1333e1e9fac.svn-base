                const dtdsanl=this.dtresult.map((item)=>{
                    return item.investFinishRate
                })
                const dtdsanlpjsz=dtname.map((item,index)=>{
                    return {//预算完成率
                        name: item,
                        value:dtdsanl[index]  
                    }
                })

                const dtdsil=this.maprows.map((item)=>{
                    return item.num3
