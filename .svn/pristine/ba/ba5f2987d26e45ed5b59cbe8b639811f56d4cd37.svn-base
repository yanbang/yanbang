<script>
  import Header from '@/components/resource_visual/header';
  import Footer from '@/components/resource_visual/footer';
  import echarts_init from './echart_lines';
  import vueSeamlessScroll from 'vue-seamless-scroll'
  import store from 'store'
  import {mapState} from 'vuex'
  import echarts_zq from "../echarts_component/echarts_zq";
  import  serviceApi from '@/api/broadband/wyceo'
  var Qs = require('@/utils/qs.js')
  var _ = require('@/utils/lodash.js')
  import {warnList} from "@/api/broadband/warn";
  var self = null
  export default {
    data() {
      return {
        zhyData: {
          name1: '',
          name2: '',
          name3: '',
          name4: '',
          value1: '',
          value2: '',
          value3: '',
          value4: '',


        },
        isDialogShow_ZHY: false,
        dloading: false,
        isDialogShow: false,
        rt1status: 1,
        gisTitle: '山东省',
        cityCode: '370000',
        iframeSrc: '',
        wcxnStatus: '1',
        wcxnStatus2: '1',
        rowttabStatus: '1',
        tabstatus: '0301',
        gisQueryParams: {},
        queryParams: {},
        gisMapData: {},
        mapLevel: 1,
        areaCode: '',
        tabdata: [
          {title: '网络能力', href: ''},
          {title: '综合预警', active: true, href: '/resource_visual/broadband_ceo'},
          {title: '综合效能', active: false, href: '/resource_visual/broadband_zhxn'},
          {title: '宽带赋能', active: false, href: '/resource_visual/jsc_kdfn'},
          {title: '存量经营', active: false, href: ''}
        ],
        ftphList: [],
        cityList: [],
        kd_xz_rate: [],
        kd_yz_rate: [],
        kd_fz_rate: [],
        gh_fz_rate: [],
        yw_fz_rate: [],
        zj_intime_rate: [],
        zj_hurry_rate: [],
        zj_linlk_intime_rate: [],
        gz_intime_rate: [],
        gz_cf_rate: [],
        gz_hurry_rate: [],
        gz_linlk_intime_rate: [],
        averageData: {},
        yhgzDatas: [],
        ztgdDatas: [],
        charts3Datas: [],
        zyyjData: {},
        tableData1: [],
        tcolums1: [
          {title: '地市', filed: 'name'},
          {title: '一次自动核配成功率', filed: 'addres_match', width: 'auto', sortable: true},
          {title: '标准地址与设备未关联', filed: 'addres_out', width: 'auto', sortable: true},
          {title: '未落图小区数量', filed: 'wlt_cell_cnt', width: 'auto', sortable: true},
          {title: '未落图包区数量', filed: 'wlt_baoqu_cnt', width: 'auto', sortable: true},
          {title: '未落图设备ftth数量', filed: 'wlt_ftth_cnt', width: 'auto', sortable: true},
        ],
        control: [],
        initgisTitle: '',

      }
    },
    components: {
      Header,
      Footer,
      vueSeamlessScroll
    },
    created() {
      self = this;
    },
    mounted() {
      var self = this;
      var initArea_id = '370000'
      this.mapLevel = 1;
      if (store.get('userInfo')) {
          var userInfo =  store.get('userInfo')
          var mapLevel = userInfo.levelAreaLevel;
          if (mapLevel == '1') {
            // 全省
            initArea_id = '370000'
            self.gisTitle  = '山东省'
            self.mapLevel = '1';
            self.control = []
          } else if (mapLevel == '2') {
            // 地市
            initArea_id = '370000'
            self.gisTitle  = '山东省'
            self.mapLevel = '1';
            self.control = ['370000', userInfo.levelAreaCode]
          } else if (mapLevel == '3') {
            // 区县
            initArea_id = userInfo.levelAreaCode;
            self.mapLevel = '2';
            self.gisTitle  = userInfo.levelAreaName
            self.control = ['370000', userInfo.levelAreaCode, userInfo.levelCityCode]
          } else if (mapLevel >= '4') {
            // 营服
            initArea_id = userInfo.levelCityCode;
            self.gisTitle  = userInfo.levelCityName
            self.mapLevel = '3';
            self.control = ['370000', userInfo.levelAreaCode, userInfo.levelCityCode, userInfo.levelYfCode]
          }
        }
      this.cityCode = initArea_id;
      this.initGisMap(initArea_id);
      // 注册地图事件
      this.initgisTitle = this.gisTitle + '';
      window.addEventListener('message', self.gismapListener, true)
      this.serverInit()
    },
    watch: {
      wcxnStatus_all (val) {
        this.renderZtgdChart()
      },
      rt1status (val) {
        this.renderDfiChart()
      }
    },
    computed: {
      ...mapState({
        searchDate: state => state.resource.searchDate,
      }),
      wcxnStatus_all () {
        return this.wcxnStatus + this.wcxnStatus2
      }
     
    },

    methods: {
      serverInit () {
        this.serviceKdYhgz()
        this.serviceKdTsl()
        this.serviceKdZyyj()
        this.serviceOtltfl()
      },
      zyyjDetailFn () {
        this.isDialogShow = true;
        this.getDetailList()
      },
      getDetailList(){
        var cityCode = this.cityCode
         this.tableData1 = []
         this.dloading = true
        warnList(cityCode).then(response => {
          this.dloading = false
          var datal = response.filter(item => {
            return item.code != '0000'
          })
          datal.forEach(item => {
             Object.keys(item).forEach(key => {
               Number(item[key]) ? item[key] =  Number(item[key]) : item[key];
            })
          });
          this.tableData1 = datal;
        })
      },

      // OLT设备退服率
      serviceOtltfl () {
        var param = {
          "cityCode": this.cityCode
        };
        serviceApi.serviceOtltfl(param).then(res => {
            if (res.rspCode == '0000') {
              var response = res.result.data || []
              var datal = response.filter(item => {
                return item.code != '0000'
              })
              var avrData = response.filter(item => {
                return item.code == '0000'
              })[0] || {}

             var orderData = this.orderDataFn(datal, 'olt_retrun_rate');
              this.zhyData.name1 = orderData[0].name
              this.zhyData.value1 = orderData[0].olt_retrun_rate + '%'
              var chartParams = {
                domId: 'chart_line1',
                legend: ['OLT端口退服数量', 'OLT退服率'],
                name: 'OLT设备退服率',
                color: 'rgba(237,161,54, 0.8)',
                bar_markLine : avrData.olt_port_return_nums, 
                xData: orderData.map(item => {
                  return item.name
                }), 
                barData:  orderData.map(item => {
                  return item.olt_port_return_nums
                }), 
                line_markLine: avrData.olt_retrun_rate,  
                lineData:  orderData.map(item => {
                  return item.olt_retrun_rate
                }), 

              }
              echarts_init.initEchart_barLine(chartParams)



              // 低效资源量分布
              var datal2 = response.filter(item => {
                return item.code != '0000'
              })
              var avrData2 = response.filter(item => {
                return item.code == '0000'
              })[0] || {}

             var orderData2 = this.orderDataFn(datal2, 'no_fgq_nums');
              var orderData_lldk = this.orderDataFn(datal2, 'no_port_nums');
             this.zhyData.name2 = orderData_lldk[0].name
             this.zhyData.value2 = orderData_lldk[0].no_port_nums 

             this.zhyData.name3 = orderData2[0].name
             this.zhyData.value3 = orderData2[0].no_fgq_nums 

              var chartParams2 = {
                domId: 'chart_line2',
                legend: ['0流量端口数量', '0占用分光器数量'],
                name: '低效资源量分布',
                unit: '数量',
                color: 'rgba(237,161,54, 0.8)',
                bar_markLine :  Number(avrData2.no_port_nums), 
                xData: orderData2.map(item => {
                  return item.name
                }), 
                barData:  orderData2.map(item => {
                  return item.no_port_nums
                }), 
                line_markLine: Number(avrData2.no_fgq_nums) || 0, 
                lineData:  orderData2.map(item => {
                  return item.no_fgq_nums
                }), 

              }
              echarts_init.initEchart_barLine(chartParams2)
              
              

            
            } else {
            this.$message.error(res.rspMsg)
            };
        })
      },

      renderZtgdChart () {
        var status = this.wcxnStatus_all
        var rrparams2 = {
          domId: 'chart_right3',
          name: '在途工单统计',
          xData: [],
          color: '',
          barData: [],
        }
        var datal = []
        if (status == '11') {
          datal = this.orderDataFn(this.ztgdDatas, 'ts_yue')
          rrparams2.barData = datal.map(item => {
            return item.ts_yue;
          })
        } else if (status == '12') {
          datal = this.orderDataFn(this.ztgdDatas, 'zyj_cs_yue')
          rrparams2.barData = datal.map(item => {
            return item.zyj_cs_yue;
          })
        }  else if (status == '13') {
          datal = this.orderDataFn(this.ztgdDatas, 'intransit_mon')
          rrparams2.barData = datal.map(item => {
            return item.intransit_mon;
          })
        }else if (status == '21') {
           datal = this.orderDataFn(this.ztgdDatas, 'ts_leiji')
          rrparams2.barData = datal.map(item => {
            return item.ts_leiji;
          })
        } else if (status == '22') {
           datal = this.orderDataFn(this.ztgdDatas, 'zyj_cs_leiji')
          rrparams2.barData = datal.map(item => {
            return item.zyj_cs_leiji;
          })
        } else if (status == '23') {
           datal = this.orderDataFn(this.ztgdDatas, 'intransit_count')
            rrparams2.barData = datal.map(item => {
              return item.intransit_count;
            })
        };
        rrparams2.xData = datal.map(item => {
          return item.name;
        })
        echarts_init.initEchart_sbar(rrparams2)
      },
       // 在途装机投诉量 
      serviceKdTsl () {
        var param = {
          "cityCode": this.cityCode
        };
        this.ztgdDatas = []
        serviceApi.serviceKdTsl(param).then(res => {
            if (res.rspCode == '0000') {
              var response = res.result.data || []
              var datal = response.filter(item => {
                return item.code != '0000'
              })
              var avdatal = response.filter(item => {
                return item.code == '0000'
              })
              this.ztgdDatas = datal;
              this.renderZtgdChart()
            
            } else {
            this.$message.error(res.rspMsg)
            };
        })
      },
       // 宽带用户感知信息
      serviceKdYhgz () {
        var param = {
          "cityCode": this.cityCode
        };
        
        serviceApi.serviceKdYhgz(param).then(res => {
            if (res.rspCode == '0000') {
              var response = res.result.data || []
              var datal = response.filter(item => {
                return item.code != '0000'
              })
              var avdatal = response.filter(item => {
                return item.code == '0000'
              })
              this.ftphList = datal;
              this.averageData = avdatal.length > 0 ? avdatal[0] : {}
              this.renderGkgzChart()
            
            } else {
            this.$message.error(res.rspMsg)
            };
        })
      },

      orderDataFn (datas, key) {
        return  _.orderBy(datas, function (item) {
            return Number(item[key])  || 0;
        }, ['desc']);
      },

      // 公客感知
      renderGkgzChart () {
        var ftphList = this.ftphList;
        var tabstatus = this.rowttabStatus;
        var Xdata = ftphList.map(item => {
          return item.name
        });

          let charts3 = {
            domId: 'chart_line4',
            unit: '百分比',
            names: ['公客履约率', '联系客户及时率', '催单率', '7天重复障碍率'],
            lineX: Xdata,
            value: [],
            bar_markLine: [],

          }
          if (tabstatus == '1') {
            // 装机
            var keyarr = ['zj_intime_rate', 'zj_linlk_intime_rate', 'zj_hurry_rate']
            var orderData1 = this.orderDataFn(ftphList, 'zj_intime_rate');
            
            charts3.names = ['公客履约率', '联系客户及时率', '催单率'],
            charts3.Xdata = orderData1.map(item => {
              return item.name
            });
            charts3.value = keyarr.map(key => {
              return orderData1.map(item => {
                    return item[key]
                })
            })
            charts3.bar_markLine = [this.averageData.zj_intime_rate, this.averageData.zj_linlk_intime_rate, this.averageData.zj_hurry_rate ]
          } else  if(tabstatus == '2') {
            // 修机
            var orderData2 = this.orderDataFn(ftphList, 'gz_hurry_rate');
            charts3.Xdata = orderData2.map(item => {
              return item.name
            });
            var keyarr = [ 'gz_linlk_intime_rate', 'gz_hurry_rate', 'gz_cf_rate']
            charts3.names = [ '联系客户及时率', '催单率', '7天重复障碍率'],
            charts3.value = keyarr.map(key => {
              return orderData2.map(item => {
                    return item[key]
                })
            })
            charts3.bar_markLine = [this.averageData.gz_linlk_intime_rate, this.averageData.gz_hurry_rate, this.averageData.gz_cf_rate ]
          } 
          echarts_init.initechart_lines(charts3);
      },

      // 端网业不匹配占比 FTTH劣化
      renderDfiChart () {
          var tabstatus = this.rt1status;
          var datal = this.charts3Datas;
          var avrData = this.zyyjData;
          if (tabstatus == 1) {
            var orderData = this.orderDataFn(datal, 'duanwang_out');
            this.zhyData.name4 = orderData[0].name
            this.zhyData.value4 = orderData[0].duanwang_out + '%'
            var chartParams = {
              domId: 'chart_line3',
              name: '端网业不匹配占比',
              color: 'rgba(237,161,54, 0.8)',
              line_markLine:Number(avrData.duanwang_out), 
              xData: orderData.map(item => {
                return item.name
              }), 
              data:  orderData.map(item => {
                return item.duanwang_out
              }), 
            }
            echarts_init.initEchart_singLine(chartParams)
          } else if (tabstatus == 2) {
             var orderData = this.orderDataFn(datal, 'ftth_out');
             var chartParams = {
              domId: 'chart_line3',
              name: 'FTTH劣化率',
              color: 'rgba(237,161,54, 0.8)',
              line_markLine:Number(avrData.ftth_out), 
              xData: orderData.map(item => {
                return item.name
              }), 
              data:  orderData.map(item => {
                return item.ftth_out
              }), 
            }
            echarts_init.initEchart_singLine(chartParams)
          }  else if (tabstatus == 3) {
             var orderData = this.orderDataFn(datal, 'iptv_out_rate');
             var chartParams = {
              domId: 'chart_line3',
              legend: ['IPTV质差用户量', 'IPTV质差用户占比'],
              name: 'FTTH劣化率',
              color: 'rgba(237,161,54, 0.8)',
              bar_markLine : avrData.iptv_out_num, 
              xData: orderData.map(item => {
                return item.name
              }), 
              barData:  orderData.map(item => {
                return item.iptv_out_num
              }), 
              line_markLine: avrData.iptv_out_rate,  
              lineData:  orderData.map(item => {
                return item.iptv_out_rate
              }), 

            }
            echarts_init.initEchart_barLine(chartParams)
          }


        


      },
      // 5.11.3FTTH劣化率、IPTV质差指标、资源预警
      serviceKdZyyj () {
        var param = {
          "cityCode": this.cityCode
        };
        this.charts3Datas = [],
        this.zyyjData = {}
        serviceApi.serviceKdZyyj(param).then(res => {
           if (res.rspCode == '0000') {
              var response = res.result.data || []
              var datal = response.filter(item => {
                return item.code != '0000'
              })
              var avdatal = response.filter(item => {
                return item.code == '0000'
              })
              this.charts3Datas = datal;
              this.zyyjData = avdatal[0] || {}
              this.renderDfiChart()

               var pieparams1 = {
                  domId: 'anipie1',
                  unit: '%',
                  value: this.zyyjData.addres_match || 0 ,
                }
                echarts_init.init_gauge(pieparams1)

                var pieparams2 = {
                  domId: 'anipie2',
                   value: this.zyyjData.addres_out || 0 ,

                }
                echarts_init.init_gauge(pieparams2)

                var pieparams2 = {
                  domId: 'anipie3',
                  value: this.zyyjData.wlt_cell_cnt || 0 ,

                }
                echarts_init.init_gauge(pieparams2)

                var pieparams2 = {
                  domId: 'anipie4',
                   value: this.zyyjData.wlt_baoqu_cnt || 0 ,

                }
                echarts_init.init_gauge(pieparams2)

                var pieparams2 = {
                  domId: 'anipie5',
                  value: this.zyyjData.wlt_ftth_cnt || 0 ,

                }
                echarts_init.init_gauge(pieparams2)

            } else {
             this.$message.error(res.rspMsg)
           };
        })
      },

      wcxntabsClick(type) {
        this.wcxnStatus = type
      },
      rowttabsClick(type) {
        this.rowttabStatus = type;
        this.renderGkgzChart()
      },
      gismapListener(e) {
        var self = this;
        if (e.data.message == 'levelSwitch') {
          self.gisTitle = e.data.area_name ||  this.initgisTitle;
          self.mapLevel = e.data.level
          self.areaCode = e.data.area_id;
          self.gisMapData = e.data;
          self.gisQueryParams = Object.assign({}, self.gisQueryParams, e.data)
          self.gisQueryParams.area_name = '';
          console.log('message', e.data)
          self.cityCode = (e.data.area_id || '370000')
          this.serverInit()
        }
      },
      initGisMap(initArea_id) {
        // 初始化地图参数
        this.gisQueryParams = {
          control: this.control.join(','),
          level: this.mapLevel,
          sceneid: 3,
          gis_sceneid: '0501',
          area_id: initArea_id || '370000',
          poi_city_id: '',
          system_type: 'C40',
          login_no: store.get('staffId') || '',
          timest: this.$store.state.resource.searchDate
        };
        var gisQueryParams = JSON.parse(JSON.stringify(this.gisQueryParams))
        this.iframeSrc = window.gis_iframeSrc +  `/smartEyes?${Qs.stringify(gisQueryParams)}`;

      }
    },
    destroyed() { // 在组件生命周期结束的时候销毁。
      window.removeEventListener('message', this.gismapListener, true)
    }
  }
</script>

