<template>

  <div class="container">
    <!-- 页头 -->
    <Header :isiframepage = true ref="">
      <template v-slot:smallTitle>
        <div class="header-smallTitle">宽带端到端-{{$route.query.title}}<span v-show="$route.query.mapLevel!=='3'">公司</span></div>
      </template>
    </Header>
    <!-- 内容 -->
    <FlexBox  style="margin-top: -10px">
      <div style="width: 100%;position: relative;">
        <!-- timestType  T T-2 -->
        <div class = "tagtabs">
          <div class="tabsbox">
            
            <div class="tabs-item " :class="{'active': timestType == 'T' }" @click="ywtjClick('T')">
              <div class="tab_borderleft"></div>
              <div class="tabfon">月累(截止当天)</div>
              <div class="tab_borderright"></div>
            </div>

            <div class="tabs-item" :class="{'active': timestType == 'T-2' }" @click="ywtjClick('T-2')">
              <div class="tab_borderleft"></div>
              <div class="tabfon">月累(截止2天前)</div>
              <div class="tab_borderright"></div>
            </div>
           
          </div>
        </div>
        <el-row :gutter="20" style="margin-top: 15px;">
          <el-col :span="12">
            <top-data title="sss" @click.native="showFixData('1')" :datad="leftData" :title-data="rightTitleAndData1">
            </top-data>

          </el-col>
          <el-col :span="12">
            <top-data title="sss"  @click.native="showFixData('0')" :datad="rightData" :title-data="rightTitleAndData2">
            </top-data>
          </el-col>
        </el-row>
        <el-row :gutter="20" style="margin-top: 15px">
          <el-col :span="24">
            <center-top :args-data="centerTopData"
                        @getbndealdata="lzBtBDoingDtaM"
                        title="sss" style="height: 228px">
            </center-top>
          </el-col>
        </el-row>
        <el-row :gutter="20" style="margin-top: 15px">
          <el-col :span="24">
            <center-data :args-data="centerData"
                         @getbndealdatad="getBtBUndealData"
                         title="sss" style="height: 228px">
            </center-data>
          </el-col>
        </el-row>
        <el-row :gutter="20" style="margin-top: 15px">
          <el-col :span="24">
            <bottom-data :args-data="bottomData" title="sss" style="height: 228px">
            </bottom-data>
          </el-col>
        </el-row>
      </div>
    </FlexBox>
    <fix-data-dialog v-if="showFixDataDalog" :title="selectTitle" :args-data="resouceDetailData" @changeShow="closeChild">
    </fix-data-dialog>
  </div>
</template>

<script>
import CenterTop from '@/views/BToB/components/centerTop'
import TopData from '@/views/BToB/components/topData'
import Header from '@/components/BigScreen/Header'
import FlexBox from '@/components/BigScreen/FlexBox'
import centerData from '@/views/BToB/components/centerData'
import BottomData from '@/views/BToB/components/bottomData'
import resourceApi from '@/api/resource'
import FixDataDialog from '@/views/BToB/components/fixDataDialog'
export default {
  name: "index",
  components: {
    BottomData,
    TopData,
    CenterTop,
    Header,
    FlexBox,
    FixDataDialog,
    centerData,
  },
  data(){
    return{
      timestType: 'T',
      showFixDataDalog:false,
      regionallevel:"",
      selectTitle:'',
      rightTitleAndData1:{
        title:'装机工单',
        dateTitle:['受理量','竣工量','在途量','注销率','24小时竣工率','48小时竣工率'],
        chatName:'接触率'
      },
      rightTitleAndData2:{
        title:'移机工单',
        dateTitle:['受理量','竣工量','在途量','注销率','24小时竣工率','48小时竣工率'],
        chatName:'取号率'
      },
      //
      leftData:{},
      rightData:{},
      centerTopData:{},
      centerData:{},
      bottomData:{},
      time:'',
      params:{},
      resouceDetailData:[],
      type:'1',
      componyName:"公司"
    }
  },
  watch:{
    $route(){
      let params2 = {
        "timest": this.$route.query.timest,
        "eparchyCode": this.$route.query.eparchyCode,
        "areaCode": this.$route.query.areaCode,
        "gridId": this.$route.query.gridId,
      }
      if (this.$route.query.gridId&&this.$route.query.gridId!==''){
        this.componyName = ""
      }
      this.params = params2;
      this.getBtBResouceData(this.params)
      this.getBtBUndealData('1')
      this.lzBtBPintDataM(this.params)
      this.lzBtBDoingDtaM('1')
    }
  },
  mounted() {
    // alert (JSON.stringify(this.$route.query.title))
    let params2 = {
      "timest": this.$route.query.timest,
      "eparchyCode": this.$route.query.eparchyCode,
      "areaCode": this.$route.query.areaCode,
      "gridId": this.$route.query.gridId,
      "timestType": this.timestType,
    }
    if (this.$route.query.gridId&&this.$route.query.gridId!==''){
      this.componyName = ""
    }
    this.params = params2;
    this.getBtBResouceData(this.params)
    this.getBtBUndealData('1')
    this.lzBtBPintDataM(this.params)
    this.lzBtBDoingDtaM('1')

  },
  methods:{
    ywtjClick(val) {
      this.timestType = val;
      this.params.timestType = val
      this.getBtBResouceData( this.params)
      this.getBtBUndealData( this.type)
      this.lzBtBPintDataM( this.params)
      this.lzBtBDoingDtaM(this.type )
    },
    showFixData(type){
      if (type === '1'){
        this.type = type
        this.selectTitle = '装机工单'
      }else {
        this.selectTitle = '移机工单'
      }
      this.getResouceDetail(type)
      this.showFixDataDalog = true
    },
    closeChild(){
      this.showFixDataDalog = false
    },
    getDate(date){
      
      this.time = date
      let params2 = {
        "timest": this.time,
        "eparchyCode": this.$route.query.eparchyCode,
        "areaCode": this.$route.query.areaCode,
        "gridId": this.$route.query.gridId,
         "timestType": this.timestType
      }
      this.params = params2
      this.getBtBResouceData( this.params)
      this.getBtBUndealData( this.type)
      this.lzBtBPintDataM( this.params)
      this.lzBtBDoingDtaM(this.type )
    },
    getResouceDetail(type){
      this.params.actionType = type
      this.type = type
      resourceApi.getBtBResouceDetai(this.params).then(data => {
        if (data.rspCode = '0000' ) {
          if ( data.result && data.result.data.length>0) {

            this.resouceDetailData = data.result.data
          } else {

          }
        }
      })
    },
    //获取数据
    getBtBResouceData(params){
      resourceApi.getBtBResouce(this.params).then(data => {
        if (data.rspCode = '0000' ) {
          if ( data.result && data.result.zj.length>0 && data.result.yj.length>0 ) {
            this.leftData = data.result.zj[0]
            this.rightData = data.result.yj[0]
          } else {
            this.leftData = {}
            this.rightData = {}
          }
        }
      })
    },
    //获取数据
    getBtBUndealData(actionType){
      this.params.actionType = actionType
      this.type = actionType
      resourceApi.getBtBUndeal(this.params).then(data => {
        if (data.rspCode = '0000') {
          if (data.result.data) {
            this.centerData = data.result.data
          } else {
            this.centerData = {}
          }
        }
      })
    },
    //获取数据
    lzBtBPintDataM(params){
      resourceApi.lzBtBPintData(this.params).then(data => {
        if (data.rspCode = '0000') {
          if (data.result.data) {
            this.bottomData = data.result.data;
          } else {
            this.bottomData = {}
          }
        }
      })
    },
    //获取数据
    lzBtBDoingDtaM(actionType){
      this.params.actionType = actionType
      this.type = actionType
      resourceApi.lzBtBDoingDta(this.params).then(data => {
        if (data.rspCode = '0000') {
          if (data.result.data) {
            this.centerTopData = data.result.data
          } else {
            this.centerTopData = {}
          }
        }
      })
    },


  }
}
</script>

