.wheel-button {
    position: relative;
}

.wheel {
    margin-top: -62px;
    padding: 0px;
    list-style: none;
    width: 280px;
    /* this will determine the diameter of the circle  */
    height: 280px;
    /* this will determine the diameter of the circle  */
    visibility: hidden;
    position: relative;
    display: none;
}

.wheel li {
    overflow: hidden;
    float: left;
}

.wheel li a {
    display: block;
}

* {
    list-style: none;
}

html,
body,
#app {
    width: 100%;
    height: 100%;
    margin: 0px;
    padding: 0px;
    background-image: url(../img/bg_sd1.png);
    background-repeat: no-repeat;
    background-size: 100% 100%;
    overflow: hidden;
}

.el-main {
    width: 100%;
    height: 1020px;
    margin: 0px;
    padding-top: 0px;
    overflow: hidden;
}

.el-header {
    background-image: url(../img/bg_sd2.png);
    background-repeat: no-repeat;
    background-size: cover;
    color: #333;
    text-align: center;
    line-height: 64px;
}

#xuanzhun1 {
    width: 460px;
    height: 460px;
    position: absolute;
    background-image: url(../img/bg_sd3-1.png);
    background-repeat: no-repeat;
    background-size: cover;
    cursor: pointer;
    z-index: 99;
}

#xuanzhun2 {
    width: 460px;
    height: 460px;
    position: absolute;
    z-index: 12;
    background-image: url(../img/bg_sd3-2.png);
    background-repeat: no-repeat;
    background-size: cover;
}

#xuanzhun3 {
    width: 400px;
    height: 400px;
    margin: 30px;
    position: absolute;
    z-index: 10;
    background-image: url(../img/bg_sd3-4.png);
    background-repeat: no-repeat;
    background-size: cover;
    -webkit-animation: rotate 15s linear infinite;
    -moz-animation: rotate 15s linear infinite;
    -o-animation: rotate 15s linear infinite;
    animation: rotate 15s linear infinite;
}

#xuanzhun4 {
    width: 460px;
    height: 460px;
    position: absolute;
    z-index: 19;
    background-image: url(../img/bg_sd3-3-s.png);
    background-repeat: no-repeat;
    background-size: cover;
    -webkit-animation: twinkling 1s infinite ease-in-out;
    -webkit-animation-duration: twinkling 1s infinite ease-in-out;
    -o-animation: twinkling 1s infinite ease-in-out;
}

#xuanzhun1 {
    -webkit-transition-property: -webkit-transform;
    -webkit-transition-duration: 1s;
    -moz-transition-property: -moz-transform;
    -moz-transition-duration: 1s;
    -webkit-animation: rotate 5s linear infinite;
    -moz-animation: rotate 5s linear infinite;
    -o-animation: rotate 5s linear infinite;
    animation: rotate 5s linear infinite;
}

@-webkit-keyframes rotate {
    from {
        -webkit-transform: rotate(0deg)
    }
    to {
        -webkit-transform: rotate(360deg)
    }
}

@-moz-keyframes rotate {
    from {
        -moz-transform: rotate(0deg)
    }
    to {
        -moz-transform: rotate(360deg)
    }
}

@-o-keyframes rotate {
    from {
        -o-transform: rotate(0deg)
    }
    to {
        -o-transform: rotate(360deg)
    }
}

@keyframes rotate {
    from {
        transform: rotate(0deg)
    }
    to {
        transform: rotate(360deg)
    }
}

#xuanzhun2 {
    -webkit-transform: rotate(360deg);
    animation: rotation 5s linear infinite;
    -moz-animation: rotation 5s linear infinite;
    -webkit-animation: rotation 5s linear infinite;
    -o-animation: rotation 5s linear infinite;
}

@-webkit-keyframes rotation {
    from {
        -webkit-transform: rotate(0deg);
    }
    to {
        -webkit-transform: rotate(-360deg);
    }
}

.guangxiao {
    width: 560px;
    height: 389px;
    margin-top: 100px;
    margin-left: -50px;
    position: absolute;
    z-index: 20;
    background-image: url(../img/bg_sd4.png);
    background-repeat: no-repeat;
    background-size: cover;
    -webkit-animation: twinkling 1s infinite ease-in-out;
    -webkit-animation-duration: twinkling 1s infinite ease-in-out;
    -o-animation: twinkling 1s infinite ease-in-out;
}

.bg_sd3-g {
    width: 400px;
    height: 400px;
    margin: 30px;
    position: absolute;
    z-index: 5;
    background-image: url(../img/bg_sd3-g.png);
    background-repeat: no-repeat;
    background-size: cover;
    -webkit-animation: twinkling 1s infinite ease-in-out;
    -webkit-animation-duration: twinkling 1s infinite ease-in-out;
    -o-animation: twinkling 1s infinite ease-in-out;
}

.bg_sd3-zi {
    width: 284px;
    height: 126px;
    margin: 460px auto auto 120px;
    position: absolute;
    z-index: 21;
    background-image: url(../img/bg_sd3-zi.png);
    background-repeat: no-repeat;
    background-size: 80% 80%;
}

@-webkit-keyframes twinkling {
    0% {
        opacity: 0.1;
    }
    100% {
        opacity: 1;
    }
}

@keyframes twinkling {
    0% {
        opacity: 0.5;
    }
    100% {
        opacity: 1;
    }
}