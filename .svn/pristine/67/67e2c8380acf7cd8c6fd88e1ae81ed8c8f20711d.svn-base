import request from '@/utils/request';
const api = {
    serviceDgwGm: '/ZhyDgwCon/serviceDgwGm', //光猫、路由数量及不匹配率统计
    serviceDgwJdh: '/ZhyDgwCon/serviceDgwJdh', //机顶盒不匹配情况
    serviceYw234: '/ZhyDgwCon/serviceYw234', //移网3G/4G/5G 用户占比
    serviceDgwPhonedetail: '/ZhyDgwCon/serviceDgwPhonedetail' //5.4.1.4宽带账号下异网手机号码详单
}

var serviceApi = {}
Object.keys(api).forEach(key => {
    serviceApi[key] = function(parameter) {
        return request({
            url: api[key],
            method: 'post',
            data: parameter
        })
    }
})

export default serviceApi