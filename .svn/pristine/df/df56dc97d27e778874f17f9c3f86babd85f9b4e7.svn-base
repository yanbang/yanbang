  },
  components: {
    Header,
    Footer,

  },
  created() {
    self = this
  },
  mounted() {
    var self = this;
      var initArea_id = '370000'
      this.mapLevel = 1;
      if (store.get('userInfo')) {
        var userInfo =  store.get('userInfo')
        var mapLevel = userInfo.levelAreaLevel;
        if (mapLevel == '1') {
          this.control = []
        } else if (mapLevel >= '2') {
          this.control = ['370000', userInfo.levelAreaCode]
        }
     };
    this.cityCode = initArea_id;
    this.initGisMap(initArea_id)
    // 注册地图事件
    window.addEventListener('message', self.gismapListener, true)

    this.initService()
    this.initService_once()
    



     this.swiper1 = new Swiper('.swiper-container', {
        init: false,
        loop: false,
        observer:true,
        observeParents:true,
        autoplay: {
          delay: 5000,
          pauseOnMouseEnter: true,
          disableOnInteraction: false,
        },
        //  pagination: {
        //   el: '.swiper-pagination',
        //   clickable :true,
        // },
      });

      this.swiper2 = new Swiper('.swiper-container2', {
        init: false,
        loop: false,
        observer:true,
        observeParents:true,
        autoplay: {
          delay: 5000,
          pauseOnMouseEnter: true,
          disableOnInteraction: false,
        }
      });

    

  },
  watch: {
    wcxnStatus (val, oldval) {
      self['swiper' + oldval].autoplay.stop();
      this.$nextTick(() => {
        self['swiper' + val].update()
        self['swiper' + val].autoplay.start();

        if (val == 2) {
          swpchart1.$resize()
          swpchart2.$resize()
          swpchart3.$resize()
          
        }
      })
    },
    dialogShow1 (val) {
      if (!val) {
        var myVideo = document.getElementById("videoBox");
        myVideo.pause();
      }
    }
  },
  computed: {
    ...mapState({
      searchDate: state => state.resource.searchDate
    }),
    
  },
  methods: {
    
 
    initService_once () {
      this.query_idcYzyld() // 云资源池 （全省）
      this.query_idcdklyl() // 带宽及利用率
      this.query_idcLxfx()
    },  
    initService () {
      this.query_idckjnl() // 空间能力
      this.query_idcgdnl() // 供电能力
      this.query_idczlnl() // 制冷能力
      
    },
    initSortTabledata (datas) {
      var datal= _.cloneDeep(datas);
       datal.forEach(item => {
        Object.keys(item).forEach(key => {
            Number(item[key]) || Number(item[key]) === 0 ? item[key] =  Number(item[key]) : item[key];
        })
      });
    
      return datal
    },
    orderDataFn (datas, key) {
      return  _.orderBy(datas, function (item) {
          return Number(item[key])  || 0;
      }, ['desc']);
    },
    // 流向分析
    query_idcLxfx () {
      if (this.IDC_TYPE == '2') {
        return
      };
      var params = {
        code:  this.cityCode,
      };
      serviceApi.service_idc_llfx(params).then((res) => {
        if (res.rspCode == '0000') {
            var datal = []
            res.result.data.forEach(item => {
              let itemTabe = [];
              let row1 = {
                name: item.idc_name + '-总流量',
                avg_inll: item.avg_in_total,
                avg_outll: item.avg_out_total,
                max_inll: item.max_in_total,
                max_outll: item.max_out_total,
              }
              let row2  = {
                name: item.idc_name + '-省内流量',
                avg_inll: item.avg_in_inside,
                avg_outll: item.avg_out_inside,
                max_inll: item.max_in_inside,
                max_outll: item.max_out_inside,
              }
              let row3  = {
                name: item.idc_name + '-省外流量',
                avg_inll: item.avg_in_outside,
                avg_outll: item.avg_out_outside,
                max_inll: item.max_in_outside,
                max_outll: item.max_out_outside,
              }
              itemTabe = [row1, row2, row3]
              datal.push(itemTabe)
            })
            this.tableData_sw = datal;

            function getXdata(pdata) {
              var avg_in_keys = ['avg_in_dianxin', 'avg_in_liantong', 'avg_in_yidong', 'avg_in_lt9929', 'avg_in_qita'];
              var avg_out_keys = ['avg_out_dianxin', 'avg_out_liantong', 'avg_out_yidong', 'avg_out_lt9929', 'avg_out_qita'];
              var max_in_keys = ['max_in_dianxin', 'max_in_liantong', 'max_in_yidong', 'max_in_lt9929', 'max_in_qita'];
              var max_out_keys = ['max_out_dianxin', 'max_out_liantong', 'max_out_yidong', 'max_out_lt9929', 'max_out_qita'];
              var keyarrs = [avg_in_keys, avg_out_keys, max_in_keys, max_out_keys]
              var xdatas = []
              keyarrs.forEach(item => {
                
                var sxdatas = []
                item.forEach(skey => {
                  sxdatas.push(pdata[skey] || 0)
                })
                xdatas.push(sxdatas)
              })

              return xdatas
            };
         
           // ['平均入流量', '平均出流量', '最大入流量',  '最大出流量'],
            var eparams1 = {
              domId: 'schartbox1',
              xData: ['中国电信', '联通骨干', '中国移动', '联通9929', '其他运营商'],
              datas: getXdata(res.result.data[0])
            }
            swpchart1 =  new echarts_init.initEchart_bar2line(eparams1)
             var eparams2 = {
              domId: 'schartbox2',
              xData: ['中国电信', '联通骨干', '中国移动', '联通9929', '其他运营商'],
              datas: getXdata(res.result.data[1])
            }
            swpchart2 =  new echarts_init.initEchart_bar2line(eparams2)
             var eparams3 = {
              domId: 'schartbox3',
              xData: ['中国电信', '联通骨干', '中国移动', '联通9929', '其他运营商'],
              datas: getXdata(res.result.data[2])
            }
            swpchart3 =  new echarts_init.initEchart_bar2line(eparams3)
            
            this.$nextTick(() => {
               window.setTimeout(function () {
                  self['swiper1'].init()
                  self['swiper2'].init()

                  self['swiper2'].el.onmouseover = function(){
                      self['swiper2'].autoplay.stop();
                  }
                  self['swiper2'].el.onmouseout = function(){
                    self['swiper2'].autoplay.start();
                  }
               }, 300)
            })
         } else {
           this.$message.error(res.rspMsg )
        }
      })
    },
    // 1.4.7IDC带宽及利用率
    query_idcdklyl () {
      if (this.IDC_TYPE == '2') {
        return
      }
      var params = {
        code:  this.cityCode,
      };
      
      serviceApi.service_idc_outbrand(params).then((res) => {
        if (res.rspCode == '0000') {
            
             var datal = this.orderDataFn(res.result.data, 'total_outbrand_cn');
             var Xdata = datal.map(item => {
               return item.name;
             });

             var data1 = datal.map(item => {
               return item.total_outbrand_cn;
             });

             var eparams1 = {
                domId: 'chart_right1',
                xAxis: Xdata,
                unit: 'G',
                data: data1
              }
              echarts_init.initComBar(eparams1);
             
             var data21 = datal.map(item => {
               return item.in_branduse_rate;
             });
              var data22 = datal.map(item => {
               return item.out_branduse_rate;
             });
             var eparams2 = {
              domId: 'chart_right2',
              legend: ['最大流入', '最大流出'],
              xData: Xdata,
              unit: '%',
              barData: [data21, data22]
            }
            echarts_init.initEchart_bar2(eparams2)

            var orderdata1 = this.orderDataFn(datal, 'in_branduse_rate');
             var orderdata2 = this.orderDataFn(datal, 'out_branduse_rate')
             this.zhyData.name1 = orderdata1[0].name;
             this.zhyData.value1 = orderdata1[0].in_branduse_rate;
             this.zhyData.name2 = orderdata2[0].name;
             this.zhyData.value2 = orderdata2[0].out_branduse_rate;



         } else {
           this.$message.error(res.rspMsg )
        }
      })
    },
    orderDataFn (datas, key) {
      return  _.orderBy(datas, function (item) {
          return Number(item[key])  || 0;
      }, ['desc']);
    },
    // 空间能力
    query_idckjnl () {
      var params = {
        code:  this.IDC_TYPE == '1'? this.cityCode : this.IDC_ID,
        type: this.IDC_TYPE
      };
      serviceApi.service_idc_kjnl(params).then((res) => {
        if (res.rspCode == '0000') {
          this.kjnlData =  res.result.data[0] || {}
         } else {
           this.$message.error(res.rspMsg )
        }
      })
    },
    // 供电能力
    query_idcgdnl () {
      var params = {
        code:  this.IDC_TYPE == '1'? this.cityCode : this.IDC_ID,
        type: this.IDC_TYPE
      };
      this.tableData1 =[]
      serviceApi.service_idc_gdnl(params).then((res) => {
        if (res.rspCode == '0000') {
          this.tableData1 =  res.result.data || [];
          this.$nextTick(() => {
            this.$refs.gdnltable.doLayout()
          })
         } else {
           this.$message.error(res.rspMsg )
        }
      })
    },

    // 制冷能力

     query_idczlnl () {
      var params = {
        code:  this.IDC_TYPE == '1'? this.cityCode : this.IDC_ID,
        type: this.IDC_TYPE
      };
       this.tableData2 =[]
      serviceApi.service_idc_zlnl(params).then((res) => {
        if (res.rspCode == '0000') {
          var datal = this.initSortTabledata(res.result.data )
          this.tableData2 =  datal || []
          this.$nextTick(() => {
            this.$refs.zlnltable.doLayout()
          })
         } else {
           this.$message.error(res.rspMsg )
        }
      })
    },

    // 云资源池
     query_idcYzyld () {
      var params = {
        cityCode:  '370000'
      };

      serviceApi.service_idc_yzyld(params).then((res) => {
        if (res.rspCode == '0000') {
            this.yzyData =  res.result.data[0] || {}
         } else {
           this.$message.error(res.rspMsg )
        }
      })
    },



    rowttabsClick(type) {
      this.rowttabStatus = type
    },
    gismapListener(e) {
      if (e.data.message == 'levelSwitch') {
        self.gisTitle = e.data.area_name || '山东省';
        self.mapLevel = e.data.level;
        self.areaCode = e.data.area_id
        self.gisMapData = e.data
        self.gisQueryParams = Object.assign({}, self.gisQueryParams, e.data)
        self.gisQueryParams.area_name = ''
        console.log('message', e.data)
        self.cityCode = e.data.area_id || '370000';
        self.IDC_TYPE = '1'
      }  else if (e.data.message == 'changeCity') {
         self.gisTitle = e.data.area_name || '山东省'
         self.mapLevel = e.data.level
         self.areaCode = e.data.area_id;
         self.gisMapData = e.data;
         self.gisQueryParams = Object.assign({},  self.gisQueryParams, e.data)
         self.gisQueryParams.area_name = '';
         self.cityCode = e.data.area_id;
         self.IDC_ID = '';
         self.IDC_TYPE = '1'
         this.initService()
      } else if (e.data.message == 'clickStar') {
        
        self.IDC_ID  =  e.data.area_id;
        self.gisTitle = e.data.area_name;
        self.IDC_TYPE = '2';
        this.initService()
        // this.dialogShow1 = true
      }
      
    },

    initGisMap(initArea_id) {
      // 初始化地图参数
      this.gisQueryParams = {
        control: this.control.join(','),
        level: this.mapLevel,
        sceneid: 5,
        gis_sceneid: '0101',
        area_id: initArea_id,
        poi_city_id: '',
        system_type: 'C40',
        login_no: store.get('staffId') || '',
        timest: this.$store.state.resource.searchDate
      }
      var gisQueryParams = JSON.parse(JSON.stringify(this.gisQueryParams))
      this.iframeSrc = window.gis_iframeSrc + `/smartEyes?${Qs.stringify(gisQueryParams)}`
    },
  },
  destroyed() {
    // 在组件生命周期结束的时候销毁。
    window.removeEventListener('message', this.gismapListener, true)
  }
}
