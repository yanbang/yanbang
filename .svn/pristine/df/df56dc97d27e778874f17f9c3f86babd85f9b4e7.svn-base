import echarts_init from './echarts_component/echart_bardc';

import  serviceApi from '@/api/idcapi'
import axios from 'axios'
export default {
  data() {
    return {
      zhyData: {
        name1: '',
        name2: '',
        value1: '',
        value2: ''
      },
      isDialogShow_ZHY:false,
      wcxnStatus: '1',
      rt1status: '1',
      pmstatus: '1',
      dialogTitle: '',
      isDialogShow: false,
      gisTitle: '山东省',
      cityCode: '370000',
      iframeSrc: '',
      rowttabStatus: '1',
      gisQueryParams: {},
      queryParams: {},
      gisMapData: {},
      mapLevel: 1,
      areaCode: '',
      tabdata: [],

      tcolums1: [
        { title: '地市', filed: 'name', width: '120' },
        { title: '千兆用户数预警', filed: 'f1' },
