      ],
      dialogShow1: false,
      zbdetailColums: [
        {'filed': 'f1', title: '客户名称'},
        {'filed': 'f1', title: '所属行业'},
        {'filed': 'f1', title: '收入信息'},
      ],
      zbdetailData: [],
