    //获取数据
    async getchanelDataMethods(type) {
      this.params.CHNL_KIND_TYPE_CODE = '01'
      this.params.flag = type
      try {
        let res = await getChanelData(this.params)
        return res?.result?.tblList[0]
      } catch (e) {
        // this.requestFailed(err);
        return {}
      }
    },
    // 总览-移网质态发展-流失+携入
    chnl_zl_loss_movein() {
      this.params.CHNL_KIND_TYPE_CODE = '01'
      chnl_store_zl_loss_movein(this.params).then(data => {
        if (data.result) {
          let ls = data.result ? data.result.data[0] : ''
          this.tableData = [{
            title: '新发展',
            date: ls.dayLoss ? ls.dayLoss : '-',
            mon: ls.monLoss ? ls.monLoss : '-',
            year: ls.yearLoss ? ls.yearLoss : '-'
          }],
            this.tableData2 = [{
              title: '',
              date: ls.dayMovein ? ls.dayMovein : '-',
              mon: ls.monMovein ? ls.monMovein : '-',
              year: ls.yearMovein ? ls.yearMovein : '-'
            }]
        }
      })
    },
    chnl_zl_sw_low() {
      this.params.CHNL_KIND_TYPE_CODE = '01'
      chnl_store_zl_sw_low(this.params).then(data => {
        if (data.result) {
          let item = ''
          this.ywsw == 'curMon' ? item = 'mon' : item = 'year'
          let rb = data.result ? data.result.data[0] : ''
          this.rightBottomPieList = [{
            name: '三无用户',
            num: rb[`${item}Sw`] ? rb[`${item}Sw`] : '-',
            value: rb[`${item}SwRate`] ? rb[`${item}SwRate`] : '-',
          }, {
            name: '极低用户',
            num: rb[`${item}Low`] ? rb[`${item}Low`] : '-',
            value: rb[`${item}LowRate`] ? rb[`${item}LowRate`] : '-',
          }]
          setTimeout(() => {
            this.rightBottomPieList.map((i, index) => {
              this.rightBottomPieChart(index, i);
            });
          }, 0);
          console.log('right', data.result.data)
        }
      })
    },
    pieChart(index, val, item) {
      let data = {
        id: item + index,
        value: val.value
      }
      pieChart.initChart(data)
    },
    waterPoloChart(index, val) {
      let data = {
        id: 'waterPoloChart' + index,
        value: val.value,
      }
      waterPoloChart.initChart(data)
    },
    rightBottomPieChart(index, val) {
      let data = {
        id: 'rightBottomPieChart' + index,
        value: val.value,
      }
      rightBottomPieChart.initChart(data)
    },

    //点击相关模块
    clickMode(type) {
      this.changgeParams(type)
      let eparchyCode = this.userInfo.eparchyCode
      let areaCode = this.userInfo.areaCode
      let gridId = this.userInfo.gridId
      let map = {
        eparchyCode: this.userInfo.eparchyCode,
        areaCode: this.userInfo.areaCode,
        gridId: this.userInfo.gridId,
        timest: this.timest,
        se_type: this.params.se_type,
        type: this.params.type,
        level: this.mapData.level,
        area_id: eparchyCode || areaCode || gridId,
        sceneid: '3',
        gis_sceneid: '6',
        poi_city_id: '370100',
        changeType: ''
      }
      map.message = "changeType";

      window.frames[0].postMessage(map, "*");
      this.$forceUpdate()
    },
    changgeParams(type) {
      this.selectMode = type
      this.params.type = "06";
      if (type === '1') {
        this.params.se_type = "XUK0003";
        this.detail_sn = 'chnl_store_5g_dev_detail'
        this.detailCN_KPI_CODE = 'CHNL_IC5G0001'
      } else if (type === '2') {
        this.params.se_type = "XUK0001";
        this.detail_sn = 'chnl_store_yw_dev_detail'
        this.detailCN_KPI_CODE = 'CHNL_ICMOBILE0001'
      } else if (type === '3') {
        this.params.se_type = "XUK0002";
        this.detail_sn = 'chnl_store_kd_dev_detail'
        this.detailCN_KPI_CODE = 'CHNL_ICBAND0001'
      } else if (type === '5') {
        this.params.se_type = "XUK0004";
        this.detail_sn = 'chnl_store_yk_dev_detail'
        this.detailCN_KPI_CODE = 'CHNL_ICMOBILE0048'
      } else if (type === '6') {
        this.params.se_type = "XUK0005";
        this.detail_sn = 'chnl_store_yy_dev_detail'
        this.detailCN_KPI_CODE = 'CHNL_ICMOBILE0049'
      } else if (type === '7') {
        this.params.se_type = "XUK0006";
        this.detail_sn = 'chnl_store_jr_dev_detail'
        this.detailCN_KPI_CODE = 'CHNL_ICMOBILE0050'
      }
      this.params.message = "changeType";
    },
    openfiveGDialogShow() {

      this.fiveGDialogShow = true;
    },
    coloseDialog() {
      this.fiveGDialogShow = false
    },
    coloseDialog2() {
      this.ywztDialogShow = false
    },
    //弹窗
    showDialog(val) {
      if (val == 1) {
        this.fiveGDialogShow = true
      }
      if (val == 2) {
        this.dialogVisible = true
      }
      if (val == 3) {
        this.isDialog3 = true
      }
      if (val == 4) {
        this.isDialog1 = true
      }
    },

    //获取数据

  },
  destroyed() { // 在组件生命周期结束的时候销毁。
    window.removeEventListener('message', this.mapInit, true)
  }
}
