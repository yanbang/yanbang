          num: arrYw.monNew ? arrYw.monNew : '-',
          value: arrYw.monNewPctPct ? arrYw.monNewPctPct : '-',
          hb: arrYw.monHb ? arrYw.monHb : '-',
          tb: arrYw.monTb ? arrYw.monrTb : '-',
        }, {
          name: '季度完成率',
          bot_name: '年累发展',
          num: arrYw.yearNew ? arrYw.yearNew : '-',
          value: arrYw.qtNewPctPct ? arrYw.qtNewPctPct : '-',
          hb: arrYw.yearHb ? arrYw.yearHb : '-',
          tb: arrYw.yearTb ? arrYw.yearTb : '-',
        }],
        this.center_right_List = [{
          name: '当日完成率',
          bot_name: '当日发展',
          num: arrKd.dayNew ? arrKd.dayNew : '-',
          value: arrKd.dayNewPctPct ? arrKd.dayNewPctPct : '-',
          hb: ''
        }, {
          name: '月完成率',
          bot_name: '月累发展',
          num: arrKd.monNew ? arrKd.monNew : '-',
          value: arrKd.monNewPctPct ? arrKd.monNewPctPct : '-',
          hb: arrKd.monHb ? arrKd.monHb : '-',
          tb: arrKd.monTb ? arrKd.monrTb : '-',
        }, {
          name: '季度完成率',
          bot_name: '年累发展',
          num: arrKd.yearNew ? arrKd.yearNew : '-',
          value: arrKd.qtNewPctPct ? arrKd.qtNewPctPct : '-',
          hb: arrKd.yearHb ? arrKd.yearHb : '-',
          tb: arrKd.yearTb ? arrKd.yearTb : '-',
        }],

        setTimeout(() => {
          this.left_top.map((i, index) => {
            this.pieChart(index, i, 'pieChart');
          });
          this.left_bottom_chart.map((i, index) => {
            this.pieChart(index, i, 'pieBottomChart');
          });
          this.left_bottom_chart1.map((i, index) => {
            this.pieChart(index, i, 'pieBottomCharts');
          });
          this.left_bottom_chart2.map((i, index) => {
            this.pieChart(index, i, 'pieBottomChartss');
          });
          this.center_List.map((i, index) => {
            this.pieChart(index, i, 'pie_center_Chart');
          });
          this.center_right_List.map((i, index) => {
            this.pieChart(index, i, 'pie_center_right_Chart');
          });
        }, 0);
      setTimeout(() => {
        this.waterPoloList.map((i, index) => {
          this.waterPoloChart(index, i);
        });
      }, 0);
