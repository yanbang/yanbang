    chnl_store_zl_sw_low: '/general/serviceIntf/chnl_store_zl_sw_low'
}

/**
 * getChanelData func
 * parameter: {
 *     username: '',
 *     password: '',
 *     remember_me: true,
 *     captcha: '12345'
 * }
 * @param parameter
 * @returns {*}
 */
export function getChanelData(parameter) {
    return request({
        url: api.chnl,
        method: 'post',
        data: parameter
    })
}

