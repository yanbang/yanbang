import request from '@/utils/request'

const api = {

    chnl: '/chnl/hallMiddleList',  //渠道营业厅
    chnl5g: '/chnl/hall5GList', //渠道营业厅5G发展列表
    chnlDetal: '/chnlDetail/hall/detail',
    chnlDetal2: '/chnlDetail/hall/capacity',
    chnlHallTrend: '/chnlDetail/hall/trend',
    chnl_store_cnfx: '/general/serviceIntf/chnl_store_cnfx', //渠道门店产能
