import request from '@/utils/request'

const api = {
    dkzrhyd: '/assessQuality/dkzrhyd', // 质-单宽转融活跃度
    xfz8yk: '/assessQuality/xfz8yk', // 质-新发展8元卡
    xfzyhscyc: '/assessQuality/xfzyhscyc', // 质-新发展用户首次预存
}

export default api

export function dkzrhyd(parameter) {
    return request({
        url: api.dkzrhyd,
        method: 'post',
        data: parameter
    })
}

export function xfz8yk(parameter) {
    return request({
        url: api.xfz8yk,
        method: 'post',
        data: parameter
    })
}

export function xfzyhscyc(parameter) {
    return request({
        url: api.xfzyhscyc,
        method: 'post',
        data: parameter
    })
}