import R1s from '@/components/Ghjs/componets/R1s.vue'
import R1x from '@/components/Ghjs/componets/R1x.vue'
export default {
  components: { Map, GhjsL2, GhjsL3, GhjsC2, GhjsR2, GhjsR3, R1s, R1x, GhjsMap, MyPie },
  data() {
    return {
      sourceData: [],
      fiveGcount: 0,
      mylist: [],
      isActive: true,
      ischange: false,
      iscolor: false,
      mapData: 1,
      mapflag1: true,
      mapflag2: false,
      mapflag3: false,
      mydate1: [1000, 3440, 5440, 1200, 3999, 5600, 5444],
      mydate2: [6000, 5000, 4000, 2000, 1000, 3000, 6000],
      flag: 1,
      i: 1
    }
  },
  created() {
    this.r1sdata(1)

    this.timer = setInterval(() => {
      this.changeMapDate(this.i)
      this.i++
      if (this.i > 3) {
        this.i = 1
      }
    }, 10000)
  },
  beforeDestory() {
    clearInterval(this.timer)
  },

  mounted() {},
  computed: {},
  methods: {
    r1sdata(value) {
      if (value == 1) {
        this.sourceData = [
          {
            source: 0,
            source_name: '联通',
            count: 888,
            percent: null,
            value
          },
          {
            source: 1,
            source_name: '电信',
            count: 888,
            percent: null
          },
          {
            source: 2,
            source_name: '移动',
            count: 888,
            percent: null
          }
        ]

        this.fiveGcount = this.sourceData.reduce((totalPrice, item) => {
          return totalPrice + item.count
        }, 0)
      } else if (value == 2) {
        this.sourceData = [
          {
            source: 0,
            source_name: '联通',
            count: 12573,
            percent: null,
            value
          },
          {
            source: 1,
            source_name: '电信',
            count: 11942,
            percent: null
          }
        ]
        this.fiveGcount = this.sourceData.reduce((totalPrice, item) => {
          return totalPrice + item.count
        }, 0)
      } else {
        this.sourceData = [
          {
            flag: 3,
            source: 0,
            source_name: '联通',
            count: 888,
            percent: null,
            value
          },
          {
            source: 1,
            source_name: '电信',
            count: 888,
            percent: null
          }
        ]
        this.fiveGcount = this.sourceData.reduce((totalPrice, item) => {
          return totalPrice + item.count
        }, 0)
      }
      if (value == 1) {
        this.mylist = [
          { code: '4G', stock: '888', fundPost: '888' },
          { code: '3G', stock: '888', fundPost: '888' }
        ]
      } else if (value == 2) {
        this.mylist = [
          { code: '4G', stock: '48030', fundPost: '48030' },
          { code: '3G', stock: '44334', fundPost: '44334' }
        ]
      } else {
        this.mylist = [
          { code: '4G', stock: '888', fundPost: '888' },
          { code: '3G', stock: '888', fundPost: '888' }
        ]
      }
    },
    r1schange(value) {
      if (value == 1) {
        this.isActive = true
        this.ischange = false
        this.iscolor = false
      } else if (value == 2) {
        this.isActive = false
        this.ischange = true
        this.iscolor = false
      } else {
        this.ischange = false
        this.iscolor = true
        this.isActive = false
      }
    },
    changeMapDate(value) {
      this.mapData = value

      if (value == 1) {
        ;(this.mapflag1 = true), (this.mapflag2 = false), (this.mapflag3 = false)
        this.i = 1
      } else if (value == 2) {
        ;(this.mapflag1 = false), (this.mapflag2 = true), (this.mapflag3 = false)
        this.i = 2
      } else {
        ;(this.mapflag1 = false), (this.mapflag2 = false), (this.mapflag3 = true)
        this.i = 3
      }
    }
  }
}
