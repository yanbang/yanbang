  <script>
import Header from '@/components/BigScreen/Header'
import FlexBox from '@/components/BigScreen/FlexBox'
import AroundFinishCity from '../../components/FifthGeneration/AroundFinishCity'
import AroundTheCity from '../../components/FifthGeneration/AroundTheCity'
import FinancialDevelopment from '../../components/Financial/FinancialDevelopment'
import frist_histogram from '../../components/FifthGeneration/frist_histogram'
import trend from '../../components/FifthGeneration/trend'
import performance from '../../components/FifthGeneration/performance'
import {
  getTableMiddleList,
  getTableRightList,
  getProvinceCpList,
  getTrendList,
} from '@/views/Evaluate/service/service'
import store from "store";
export default {
  name: 'fifth-generation',
  components: {
    Header,
    FlexBox,
    frist_histogram,
    trend,
    AroundFinishCity,
    AroundTheCity,
    FinancialDevelopment,
    performance,
  },
  data() {
    return {
      flag: '6',
      trendList: [],
      weekStart: '',
      weekEnd: '',
      middleList: [],
      provinceCpList: [],
      provinceData: {},
      provinceDataWeek: {},
      rightList: [],
      getChildValue: '',
      bottomList: [
        {
            name: '移网发展',
            url: '/Evaluate/MoveNet',
           
          },
          {
            name: '5G发展',
            url: '/Evaluate/fifth-generation',
          },
          {
            name: '宽带发展',
            url: '/Evaluate/broadband',
           
          },
          {
            name: '融合发展',
            url: '/Evaluate/Integrated',
          },
          {
              name: '金融发展',
              url: '/Financial/Financial',
                active: true,
          },
      ],
    }
  },
  mounted() {
    if (store.get('userInfo').isAddMask == '1') {
      this.$watermark(store.get('userInfo').serialNumber);
    } else {
      this.$watermark("");
    }
    if (window.sessionStorage.getItem('selectTime')) {
      this.getChildValue = window.sessionStorage.getItem('selectTime')
      console.log(this.getChildValue)
    } else {
      this.getToday()
    }
    //this.getDate(this.getChildValue)
    // this.getProvinceCpList()
    // this.getTrendList()
    // this.getContentData()
    // this.getTableRightList()
    this.init()
  },
  methods: {
    getBeforeDate(num, time) {
      let n = num;
      let d = '';
      if(time) {
        d = new Date(time);
      } else {
        d = new Date();
      }
      let year = d.getFullYear();
      let mon = d.getMonth() + 1;
      let day = d.getDate();
      if(day <= n) {
        if(mon > 1) {
          mon = mon - 1;
        } else {
          year = year - 1;
          mon = 12;
        }
      }
      d.setDate(d.getDate() - n);
      year = d.getFullYear();
      mon = d.getMonth() + 1;
      day = d.getDate();
      let s = year + "-" + (mon < 10 ? ('0' + mon) : mon) + "-" + (day < 10 ? ('0' + day) : day);
      return s;
    },
    getDate(date) {
      this.getChildValue = date
      let y = date.substring(0, 4)
      let m = date.substring(4, 6)
      let d = date.substring(6)
      this.weekEnd = y + '-' + m + '-' + d
      this.weekStart = this.getBeforeDate(6,this.weekEnd )

      this.getProvinceCpList()
      this.getTrendList()
      this.getContentData()
      this.getTableRightList()
    },

    init() {},
    getContentData() {
      this.middleList = []
      getTableMiddleList({
        timest: this.getChildValue,
        eparchyCode: '0000',
        areaCode: '',
        gridId: '',
        flag: '6',
      })
        .then((res) => {
          if (res) {
            this.middleList = res.result.tblList
          }
        })
        .catch(() => {})
    },
    getTableRightList() {
      this.rightList = []
      getTableRightList({
        timest: this.getChildValue,
        eparchyCode: '0000',
        areaCode: '',
        gridId: '',
        flag: '6',
      })
        .then((res) => {
          if (res) {
            res.result.tblList.forEach((datas) => {
              this.rightList.push(datas)
            })
          }
        })
        .catch(() => {
          //console.log(123)
        })
    },
    //柱状图
    getProvinceCpList() {
      this.provinceCpList = []
      getProvinceCpList({
        timest: this.getChildValue,
        eparchyCode: '0000',
        areaCode: '',
        gridId: '',
        flag: '6',
      })
        .then((res) => {
          this.provinceData = res.result.quarterStatus
          this.provinceDataWeek = res.result.weekStatus
          res.result.tblList.forEach((datas) => {
            this.provinceCpList.push(datas)
          })
        })
        .catch(() => {})
    },

    getTrendList() {
      this.trendList = []
      getTrendList({
        timest: this.getChildValue,
        eparchyCode: '0000',
        areaCode: '',
        gridId: '',
        flag: '6',
      })
        .then((res) => {
          if (res) {
            let index = 0
            res.result.tblList.forEach((datas) => {
              //取七天数据
              if (index <= 6) {
                this.trendList.push(datas)
                // console.log('七天数据1' + datas)
              }
              index = index + 1
            })
          }
        })
        .catch(() => {
          //console.log(123)
        })
    },
    fatherMethod(val) {
      this.getChildValue = val
    },
    getToday() {
      let d = new Date()
      let year = d.getFullYear()
      let Mos = d.getMonth() + 1
      let Dates = d.getDate() - 2
      let youWant = year + '-' + this.addZero(Mos) + '-' + this.addZero(Dates)
      this.getChildValue = youWant.split('-').join('')
    },
    addZero(s) {
      return s < 10 ? '0' + s : s
    },
    /**
     * 格式化时间
     */
    getWeekDay(day) {
      //当前日期
      let today = new Date()
      let targetday_milliseconds = today.getTime() + 1000 * 60 * 60 * 24 * day
      today.setTime(targetday_milliseconds) //注意，这行是关键代码
      let tYear = today.getFullYear()
      let tMonth = today.getMonth()
      let tDate = today.getDate()
      tMonth = this.addZero(tMonth + 1)
      tDate = this.addZero(tDate)
      // console.log(today.setTime(targetday_milliseconds), '11111111')
      return tYear + '-' + tMonth + '-' + tDate
    },
    // doHandleMonth(month){
    //   let m = month;
    //   if(month.toString().length == 1){
    //     m = "0" + month;
    //   }
    //   return m;
    // },
  },
  watch: {},
}
</script>

