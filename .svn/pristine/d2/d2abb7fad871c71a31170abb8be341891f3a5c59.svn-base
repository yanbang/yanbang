          <img
            src="../../assets/images/bigScreen/fullService/biankuang01.png"
            alt=""
            style="position: absolute; height: 100%; width: 100%; left: 0; top: 0; z-index: 0"
          />
          <div class="center-content" style="z-index: 2; position: absolute; width: 100%; height: 100%">
            <div class="center-title">
              全省金融发展通报
              <img src="../../assets/images/bigScreen/fullService/biaoti.png" alt="" />
            </div>
            <performance :city-data="middleList"></performance>
            <AroundTheCity :middleList="middleList"></AroundTheCity>
          </div>
        </div>
        <!-- 右边 -->
        <div class="right">
          <div class="fiveG-title" style="font-size: 26px">各地市营服周完成率区间数量</div>
          <div>
            <AroundFinishCity :rightList="rightList"></AroundFinishCity>
